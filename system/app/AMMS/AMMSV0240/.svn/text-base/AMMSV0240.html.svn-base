				<div class="fieldgroupInBox">
					<div class="table mrgb0">
						<!-- セルを直接編集可能に -->
						<table class="hilight drilldown" id="ca_table">
						<thead>
							<tr>
								<th width="80px">レベル</th>
								<th width="">階層名</th>
								<th width="120px">店舗階層フラグ</th>
								<th width="60px"></th>
								<th width="60px"></th>
							</tr>
						</thead>
						<tbody id="ca_table_tbody">
						</tbody>
						<tfoot>
							<tr class="addRow">
								<td colspan="5">
									<span class="btn-add"></span>
								</td>
							</tr>
						</tfoot>
						</table>
					</div>
					<div class="clear"></div>
				</div>
			</div>

