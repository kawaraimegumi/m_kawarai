					<div  class="fieldgroupInBox setaddr">
						<input type="hidden" id="ca_orgfuncID">
						<div class="fieldUnit mrgb10 half flleft required">
							<p class="fieldName bl2">組織体系<br />コード</p>
							<div class="fieldBox">
								<span class="limit wt40 flright mrgl40 pa_orgfuncCode"></span>
								<input type="text" class="form-control wt80 ime-disabled cl_valid cl_required" id="ca_orgfuncCode" data-tflimit="2" data-validator="len:2,2 digit" readonly>
							</div>
						</div>
						<div class="clear"></div>
						<div class="fieldUnit mrgb10 half flleft required">
							<p class="fieldName">組織体系名称</p>
							<div class="fieldBox">
								<span class="limit wt40 flright mrgl240 pa_orgfuncName"></span>
								<input type="text" placeholder="" class="form-control wt280 ime-active cl_valid cl_required" data-tflimit="20" data-validator="maxlen:20" id="ca_orgfuncName"/>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
						<div class="fieldUnit">
							<p class="fieldName bl2">基本組織体系<br />フラグ</p>
							<div class="fieldBox">
								<ul>
								<li><label class="radio disabled"><input type="radio" name="ca_basicFlag" value="1" data-toggle="radio" disabled>基本組織体系</label></li>
								<li><label class="radio disabled"><input type="radio" name="ca_basicFlag" value="0" data-toggle="radio" disabled>任意組織体系</label></li>
							</ul>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
					</div>
				</div>
