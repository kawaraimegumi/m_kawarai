		<script id="ca_tbody_template" type="text/x-jquery-tmpl">
			<tr class="deletable addRow" name="ca_table_tr" id="${orglevelID}">
				<td class="editable pdg0"><input type="text" class="form-control txtar" name="levelNo"value="${levelNo}" readonly></td>
				<td class="editable pdg0"><input type="text" class="form-control cl_valid cl_required" name="orglevelName" value="${orglevelName}" data-validator="maxlen:20" {{if disEdit}}readonly{{/if}}></td>
				<td class="editable txtac"><label class="checkbox {{if storeLevelFlag}}checked{{/if}} {{if disChk}}disabled{{else disEdit}}disabled{{/if}}"><input type="checkbox" class="form-control" name="storeLevelFlag" data-toggle="checkbox" {{if storeLevelFlag}}checked{{/if}} {{if disChk}}disabled{{else disEdit}}disabled{{/if}}></label></td>
				<td>{{if canAdd}}<span class="btn-add flright"></span>{{/if}}</td>
				<td>{{if editable}}<span class="btn-delete flright"></span>{{/if}}</td>
				<input type="hidden" name="orglevelTypeID" value="${orglevelTypeID}">
				<input type="hidden" name="orglevelID" value="${orglevelID}">
			</tr>
		</script>

