<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="viewport" content="width=1024, initial-scale=1.0, maximum-scale=1.0" >
	<title>商品マスタ</title>

	<!-- Loading Bootstrap -->
	<link href="../../../bootstrap/css/bootstrap.css" rel="stylesheet">
	<link href="../../../bootstrap/css/prettify.css" rel="stylesheet">

	<link rel="stylesheet" type="text/css" href="../../../../css/jquery-ui-1.10.3.custom.css" />
	<script type="text/javascript" src="../../../../js/jquery.js"></script>
	<script type="text/javascript" src="../../../../js/jquery-ui.custom.js"></script>
	<script type="text/javascript" src="../../../../js/jquery.csv.js"></script>

	<link media="screen" rel="stylesheet" type="text/css" href="../../../css/style.css">

	<script src="../../../js/function.js"></script>

	<script src="../../../js/jquery.ui.touch-punch.min.js"></script>
	<script src="../../../js/bootstrap.min.js"></script>
	<script src="../../../js/bootstrap-select.js"></script>
	<script src="../../../js/bootstrap-switch.js"></script>
	<script src="../../../js/flatui-checkbox.js"></script>
	<script src="../../../js/flatui-radio.js"></script>
	<script src="../../../js/jquery.tagsinput.js"></script>
	<script src="../../../js/jquery.placeholder.js"></script>
	<script src="../../../js/jquery.form.js"></script>
	<script src="../../../bootstrap/js/google-code-prettify/prettify.js"></script>
	<script src="../../../js/application.js"></script>
	<script src="../../../js/jquery.skOuterClick.js"></script>
	<script src="../../../js/jquery.tablesorter.js"></script>
	<script src="../../../js/tsort.js"></script>
	<script src="../../../../js/jquery.contextmenu.js"></script>

	<script src="../../../../js/jquery.ui.datepicker.js"></script>
	<script src="../../../../js/jquery.ui.datepicker-ja.min.js"></script>
	<script type="text/javascript"
			src="../../../../js/jquery.tablefix_1.0.0.js"></script>
	<script type="text/javascript" src="../../../../js/jquery.dragtable.js"></script>
	<script type="text/javascript" src="../../../../js/jquery.blockUI.js"></script>

	<script type="text/javascript" src="../../../../js/underscore.js"></script>
	<script type="text/javascript" src="../../../../js/backbone.js"></script>
	<script type="text/javascript" src="../../../../js/json2.js"></script>
	<script type="text/javascript" src="../../../../js/jquery.tmpl.min.js"></script>
	<script type="text/javascript"
		src="../../../../js/jquery.exchangeselect.0.1.0.js"></script>
	<script type="text/javascript" src="../../../../js/backbone.syphon.js"></script>
	<script type="text/javascript" src="../../../../js/backbone.syphon.fix.js"></script>
	<script src="../../../js/clcom.js"></script>

	<link rel="stylesheet" href="../../../css/themes/blue/style.css" type="text/css">
	<link rel="stylesheet" href="../../../css/tsort/style.css" type="text/css">

<script>

</script>


</head>

<body class="cl_body_hidden">

<script src="AMMSV0480.js"></script>

<div id="ca_main">

<div id="container">

	<div id="mainColumn">
		<div id="mainColumninBox" class="noLeftColumn">

			<div id="content">

				<div class="fieldgroup" id="ca_form">
					<div class="fieldgroupInBox mrgb10">
						<div id="ca_upper">
							<div class="fieldUnit mrgb10 flleft half mrgr20 required" id="div_unitID">
								<p class="fieldName">事業ユニット</p>
								<div class="fieldBox required">
									<select name="info" class="mbn wt280 cl_valid cl_required" id="ca_unitID"></select>
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
							<div class="fieldUnit mrgb10 half flleft">
								<p class="fieldName">核商品コード</p>
								<div class="fieldBox">
									<input type="text" placeholder="" class="form-control wt280" id="ca_code" disabled/>
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
							<div class="fieldUnit mrgb10 half flleft mrgr10 required">
								<p class="fieldName">核商品名称</p>
								<div class="fieldBox">
									<span class="limit wt40 flright mrgl240" id="pa_name"></span>
									<input type="text" placeholder="" class="form-control wt280 cl_valid cl_required" data-tflimit="20" data-validator="maxlen:20 zenkaku" id="ca_name"/>
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
							<div class="fieldUnit mrgb10 flleft half mrgr20 required">
								<p class="fieldName">核商品区分</p>
								<div class="fieldBox">
									<select name="info" class="mbn wt280 cl_valid cl_required" id="ca_coreitemTypeID"></select>
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
							<div class="fieldUnit mrgb10 flleft mrgr10 ">
								<p class="fieldName">説明・メモ</p>
								<div class="fieldBox">
									<span class="limit wt40 flright mrgl620" id="pa_memo"></span>
									<input type="text" placeholder="" class="form-control cl_valid wt660 " data-tflimit="50" data-validator="maxlen:50 zenkaku" id="ca_memo"/>
								</div>
								<div class="clear"></div>
							</div>
						</div>
						<div class="clear"></div>
						<hr>
						<div class="fieldUnit hta mrgb10 flleft mrgr10 required">
							<div class="fieldUnit mrgb20 flright ">
								<div class="fieldBox noName flleft " id="ca_csvinput2">
									<button class="btn btn-default wt280 mrgl10 cl-file-attach" id="ca_csv_download">Excelサンプルダウンロード</button>
								</div>
								<div class="fieldBox noName flleft" id="ca_csvinput1">
									<input type="file" class="hide-input" id="file_csv_uptake"/>
									<button class="btn btn-default wt280 mrgl10 cl-file-attach" id="ca_csv_uptake">Excelデータアップロード</button>
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
							<p class="fieldName bl2">核商品<br />対象商品</p>
							<div class="table fieldBox mrgb10">
								<table class="hilightRow list flleft " id="ca_table">
								<thead id="ca_table_thead">
									<tr>
										<th width="140px" >品種</th>
										<th width="200px" >メーカー</th>
										<th width="200px" >メーカー品番</th>
										<th width="200px" >商品名称</th>
										<th width="80px"></th>
									</tr>
								</thead>
								<tbody id="ca_table_tbody">
								</tbody>
								<tfoot id="ca_table_tfoot">
									<tr class="addRow">
										<td colspan="6"><span class="btn-add"></span></td>
									</tr>
								</tfoot>
								</table>
								<script id="ca_rec_template" type="text/template">
									<tr name="<%- no %>" class="deletable">
										<td class="ca_c_link editable pdg0">
											<input type="text" class="form-control cl_valid cl_required" name="ca_itgrpID" />
										</td>
										<td class="ca_c_link editable pdg0">
											<input type="text" class="form-control cl_valid cl_required" name="ca_makerID" />
										</td>
										<td class="ca_c_link editable pdg0">
											<input type="text" class="form-control cl_valid cl_required" name="ca_makerItemCode" data-validator="maxlen:10 hankaku" value="<%- makerItemCode %>" />
										</td>
										<td class="ca_c_link" name="td_name">
											<%- itemName %>
										</td>
										<td class="ca_c_link" name="btn_delete"><span class="btn-delete flright"></span></td>
										<td class="ca_c_link dispn">
											<input type="hidden" name="ca_itemID" value="<%- itemID %>" />
										</td>
										<td class="ca_c_link dispn">
											<input type="hidden" name="ca_itemName" value="<%- itemName %>" />
										</td>
									</tr>
								</script>
							</div>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldgroupInBox mrgb10">
						<input type="hidden" id="ca_id" />
						<div class="fieldUnit flleft mrgb10 required">
							<p class="fieldName">対象期間</p>
							<div class="fieldBox">

								<div class="datepicker_wrap flleft">
									<input type="text" id="ca_startDate" class="cl_valid cl_required cl_date" />
								</div>
								<p class="txtInFieldUnit pdg flleft">〜</p>
								<div class="datepicker_wrap flleft">
									<input type="text" id="ca_endDate" class="cl_valid cl_required cl_date" />
								</div>
							</div>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
				</div>

			</div>
			<!-- /#content -->

			<div id="mainColumnFooter" class="x2">
			</div>
			<!-- /#mainColumnFooter -->

		</div>
		<!-- /#mainColumninBox -->

	</div>
	<!-- /#mainColumn -->

</div><!-- / #container -->

<div class="clear"></div>

</div>
</body>
</html>
