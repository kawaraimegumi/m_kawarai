<body class="cl_body_hidden">

<script src="AMDSV0020.js"></script>

<div id="ca_main">

<div id="container">

	<div id="mainColumn">
	<div id="mainColumninBox" class="noLeftColumn">

		<div id="content">

			<div class="fieldgroup" id="ca_base_form">
				<div id="ssetaddr" class="fieldgroupInBox setaddr">
					<input type="hidden" id="ca_ptnID">
					<div class="fieldUnit mrgb10 flleft half mrgr20 required">
						<p class="fieldName">事業ユニット</p>
						<div class="fieldBox" id="ca_unitID_div">
							<select name="info" class="mbn wt280 cl_valid cl_required" id="ca_unitID"></select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half flleft mrgr20 required">
						<p class="fieldName">品種</p>
						<div class="fieldBox">
							<input type="text" class="form-control wt280 cl_valid cl_required" data-validator="maxlen:24" id="ca_itgrpID"/>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half flleft mrgr20">
						<p class="fieldName bl3">店舗ランク<br />パターン<br />コード</p>
						<div class="fieldBox">
							<input type="text" class="form-control wt280" id="ca_rankPtnCode" readonly/>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 half flleft required">
						<p class="fieldName bl2">店舗ランク<br />パターン名</p>
						<div class="fieldBox">
							<span class="limit wt40 flright mrgl410"></span>
							<input type="text" class="form-control wt450 cl_valid cl_required ime-active" data-tflimit="30 zenkaku" data-validator="maxlen:30 zenkaku" id="ca_rankPtnName"/>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half flleft mrgr20">
						<p class="fieldName">サブクラス1</p>
						<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_sub1ID">
								</select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 half flleft">
						<p class="fieldName">サブクラス2</p>
						<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_sub2ID">
									<optgroup label="">
									</optgroup>
								</select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half flleft mrgr20">
						<p class="fieldName">ブランド</p>
						<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_brandID">
								</select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 half flleft">
						<p class="fieldName">スタイル</p>
						<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_styleID">
									<optgroup label="">
									</optgroup>
								</select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half flleft mrgr20">
						<p class="fieldName">色</p>
						<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_colorID">
									<optgroup label="">
									</optgroup>
								</select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 half flleft">
						<p class="fieldName">柄</p>
						<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_designID">
									<optgroup label="">
									</optgroup>
								</select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half">
						<p class="fieldName bl2">プライス<br />ライン</p>
						<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_priceLineID">
									<optgroup label="">
									</optgroup>
								</select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>

				</div>
			</div>

			<div class="fieldgroup">
				<div class="fieldgroupInBox">
					<div class="fieldUnit mrgbshort flright">
						<div class="fieldBox noName flleft" id="ca_csvinput2">
							<button class="btn btn-default wt280 mrgl10 cl-file-attach" id="ca_sample_download">Excelサンプルダウンロード</button>
						</div>
						<div class="fieldBox noName flleft">
							<button class="btn btn-default wt280 mrgl10 cl-file-attach" id="ca_csv_uptake">Excelデータアップロード</button>
						</div>
						<div class="clear"></div>
					</div>
					<hr>

                    <div class="row">
                        <div class="col-md-6">
                            <div class="tblfixhdr-container">
                                <div class="tblfixhdr-viewport" id="ca_tblfixhdr-viewport">
								    <table id="ca_tableRank" class="table hilightRow drilldown selectable">
		                                <thead>
									        <tr>
										        <th width="120px">店舗ランクコード
										        	<div>店舗ランクコード</div>
										        </th>
										        <th width="">店舗ランク名
										        	<div>店舗ランク名</div>
										        </th>
										        <th width="90px">所属店舗数
										        	<div>所属店舗数</div>
										        </th>
										        <th width="40px">&nbsp;
										        	<div>&nbsp;</div>
										        </th>
									        </tr>
		                                </thead>
									    <tbody id="ca_tableRank_tbody">
										    <!-- ca_table_tbody_template -->
									    </tbody>
									    <!-- <tfoot id="ca_tableRank_tfoot">
										    <tr class="addRow">
											    <td colspan="4">
												    <span class="btn-add"></span>
											    </td>
										    </tr>
									    </tfoot> -->
								    </table>
                                </div>
                            </div>
							<div class="cl_datagrid_footer" style="width: auto;"><span class="btn-add"></span></div>
                        </div>
                        <div class="col-md-6">
                            <div class="tblfixhdr-container">
                                <div class="tblfixhdr-viewport2">
                                    <table id="ca_tableStore" class="table hilightRow drilldown">
					                    <thead>
                                            <tr>
							                    <th>
                                                    店舗
                                                    <div>店舗</div>
                                                </th>
							                    <th width="140px">
                                                    店舗ランクコード
                                                    <div>店舗ランクコード</div>
                                                </th>
						                    </tr>
                                        </thead>
					                    <tbody id="ca_tableStore_tbody">
						                    <!-- ca_table_tbody_template -->
					                    </tbody>
				                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
					<!-- <div class="mrgb10"></div> -->
					<div class="clear"></div>
				</div>

			</div>

		</div>
		<!-- /#content -->

		<div id="mainColumnFooter" class="noLeftColumn x2">
		<!--
			<p class="cancel"><a href="javascript:void(0)" id="cl_cancel">キャンセル</a></p>
			<p class="apply mrgCancel"><a href="javascript:void(0)" id="cl_submit">登録</a></p>
			<p class="flleft"><a href="javascript:void(0)" id="cl_prev">前へ</a></p>
			<p class="flright"><a href="javascript:void(0)" id="cl_next">次へ</a></p>
			<div class="clear"></div>
		 -->
		</div>
		<!-- /#mainColumnFooter -->

	</div>
	<!-- /#mainColumninBox -->

	</div>
	<!-- /#mainColumn -->

</div><!-- / #container -->

<div class="clear"></div>

<!-- 行テンプレート: underscore.js テンプレートを使用 -->
<script id="ca_rank_template" type="text/template">
	<tr name="" class="deletable" data-rowindex="<%- rowIndex %>" height="41px">
		<td class="ca_c_link dispn"><span name="ca_rankID"><%- rankID %></span></td>
		<td class="ca_c_link editable pdg0">
			<input type="text" name="ca_rankCode" class="form-control flright cl_valid cl_required" data-validator="maxlen:3 digit" value="<%- rankCode %>"/>
		</td>
		<td class="ca_c_link editable">
			<span class="data"><%- rankName %></span>
			<span class="edit">編集</span>
			<input type="text" name="ca_rankName" class="form-control flright cl_valid cl_required dispn" data-validator="maxlen:20" value="<%- rankName %>"/>
		</td>
		<td class="ca_c_link"><span class="" name="ca_storeNum" style="float:right"><%- storeNum %></span></td>
		<td class="ca_c_link dispn"><span name="ca_f_del"><%- f_del %></span></td>
		<td class="ca_c_link"><span class="btn-delete flright"></span></td>
	</tr>
</script>
<script id="ca_store_template" type="text/template">
	<tr name="" class="deletable" height="41px">
		<td class="ca_c_link dispn"><span name="ca_storeID"><%- storeID %></span></td>
		<td class="ca_c_link dispn"><span name="ca_storeCode"><%- storeCode %></span></td>
		<td class="ca_c_link dispn"><span name="ca_storeName"><%- storeName %></span></td>
		<td class="ca_c_link dispn"><span name="ca_rankID"><%- rankID %></span></td>
		<td class="ca_c_link dispn"><span name="ca_oldRankCode"><%- oldRankCode %></span></td>
		<!-- <td class="ca_c_link dispn"><input type="hidden" name="ca_rankID" value="<%- rankID %>"></td> -->
		<td class="ca_c_link"><%- storeCode + ":" + storeName %></td>
		<td class="ca_c_link editable">
			<span class="data"><%- rankCode %></span>
			<span class="edit">ランク変更</span>
			<input type="text" name="ca_rankCode" class="form-control flright cl_valid dispn cl_valid_auto_off" data-validator="maxlen:3 digit" value="<%- rankCode %>"/>
		</td>
	</tr>
</script>

</div>

</body>
