.deleteRow:hover div:after {
    width: 12px;
    height: 12px;
    background: url("/system/images/icn_delete_active.svg") no-repeat right top;
    background: url("/system/images/icn_delete_active.png") no-repeat right top\9;
    content: "";
    display: block;
    position: absolute;
    right: 8px;
    top: 11px;
}

