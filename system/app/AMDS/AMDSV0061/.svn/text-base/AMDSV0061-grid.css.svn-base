#ca_datagrid {
    height: 600px;
}

.grid-canvas div.input {
    padding: 8px 12px;
}

.slick-cell .noedit,
.slick-cell .cell {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: 0;
    overflow: hidden;
}

.slick-cell .noedit table td {
    font-size: 12px;
    padding: 8px 12px;
}

.slick-cell .grd-tbl {
    display: table;
    width: 100%;
    height: 100%;
}

.slick-cell .grd-tbl .grd-row {
    display: table-row;
}

.slick-cell.input.active.editable {
    padding: 0;
    border-left: 1px solid #E4E6EB;
    border-top: 1px solid #E4E6EB;

}

.slick-cell .grd-tbl .grd-cell {
    display: table-cell;
    vertical-align: middle;
    padding: 8px 12px;
}

.slick-cell .grd-tbl .grd-cell.editable {
    padding: 0;
}

.slick-cell .grd-tbl .grd-cell.half {
    width: 50%;
}

.slick-cell .grd-tbl .grd-cell:not(:first-child) {
    border-left: 1px solid #E4E6EB;
}

.slick-cell .grd-cell {
    padding: 8px 12px;
}

.slick-cell .grd-cell .baseStockQy input[type=text]{
    font-size: 14px;
}

.cl_datagrid .slick-cell.input {
    background: transparent;
}

.cl_datagrid .slick-cell.input .baseStockQy .form-control {
    background: url("/system/images/bg_tdinput.png");
}

.cl_datagrid .slick-cell .pdg0 {
    padding: 0;
}

.cl_datagrid .slick-cell .sales {
    border-bottom: 1px solid #FFF;
    padding: 5px 5px;
    height: 27px;
}

.cl_datagrid .slick-cell .stock {
    padding: 5px 5px;
    height: 27px;
}

.slick-row {
    font-family: Lato, メイリオ, 'ＭＳ Ｐゴシック', sans-serif;
    font-size: 12px;
    color: #212226;
}

