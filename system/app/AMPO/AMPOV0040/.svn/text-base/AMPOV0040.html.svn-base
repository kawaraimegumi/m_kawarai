<body class="cl_body_hidden">
	<script src="AMPOV0040.js"></script>

	<div id="ca_main">

		<!-- /#header -->

		<div id="container">

			<div id="mainColumn">
				<div id="mainColumninBox" class="noLeftColumn">
					<!-- /#title -->

					<div id="content">
						<div class="fieldgroup" id="ca_base_form">
							<div class="fieldgroupInBox">
								<input type="hidden" id="ca_recno"> <input type="hidden"
									id="ca_state"> <input type="hidden" id="ca_id">
								<div class="fieldUnit mrgb10 half flleft required">
									<p class="fieldName">
										事業ユニット
									</p>
									<div class="fieldBox ca_unitID_div">
										<select name="info" class="mbn wt280 cl_valid cl_required"
											id="ca_unitID" name="unitID">
										</select>
									</div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10 half required">
									<p class="fieldName">ＰＯ種別</p>
									<div class="fieldBox ca_poTypeID_div">
										<select name="info" class="mbn wt280 cl_valid cl_required"
											id="ca_poTypeID" name="poTypeID">
											<optgroup label="">
											</optgroup>
										</select>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10 half flleft required">
									<p class="fieldName">親ブランドID</p>
									<div class="fieldBox">
									<span class="limit wt40 flright mrgl240" length="3"></span>
										<input type="text" placeholder=""
											class="form-control wt280 ime-active cl_valid cl_required"
											id="ca_code" name="code" data-tflimit="3" data-validator="maxlen:3 alnum"/>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10 half flleft required">
									<p class="fieldName bl2">
										親ブランド<br />名称
									</p>
									<div class="fieldBox">
									<span class="limit wt40 flright mrgl340" length="30"></span>
										<input type="text" placeholder=""
											class="form-control wt380 ime-disabled cl_valid cl_required"
											data-tflimit="30" data-validator="maxlen:30" name="name" id="ca_name" />
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10  flleft mrgr20 required">
									<p class="fieldName">適用開始日</p>
									<div class="fieldBox">
										<div class="datepicker_wrap ca_stDate_div">
											<input type="text" id="ca_stDate" name="stDate"
												class="cl_date cl_valid cl_required ime-disabled"
												data-limit="len:13" data-validator="maxlen:13"/>
										</div>
									</div>
									<div class="clear"></div>
								</div>
								<div class="fieldUnit mrgb10  flleft mrgr20 required">
									<p class="fieldName">適用終了日</p>
									<div class="fieldBox">
										<div class="datepicker_wrap ca_edDate_div">
											<input type="text" id="ca_edDate" name="edDate"
												class="cl_date cl_valid cl_required ime-disabled"
												data-limit="len:13" data-validator="maxlen:13"/>
										</div>
									</div>
									<div class="clear"></div>
								</div>
								<div class="fieldUnit mrgb10  flleft mrgr20">
									<p class="fieldName">発注停止日</p>
									<div class="fieldBox">
										<!-- ツールチップでヘルプを表示 -->
										<p class="txtInFieldUnit flright help" id="ca_tp_code"  title="発注停止日：この日付以降店舗ではそのマスタを参照不可になります。<br />適用終了日：店舗だけでなく本部でもそのマスタを参照不可になります。"><span>?</span></p>
										<div class="datepicker_wrap ca_ordStopDate_div">
											<input type="text" id="ca_ordStopDate" name="ordStopDate"
												class="cl_date cl_valid  ime-disabled" data-limit="len:13" data-validator="maxlen:13"/>
										</div>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>
							</div>
						</div>

					</div>
					<!-- /#content -->
					<div id="mainColumnFooter" class="noLeftColumn x2"></div>
					<!-- /#mainColumnFooter -->

				</div>
				<!-- /#mainColumninBox -->

			</div>
			<!-- /#mainColumn -->

		</div>
		<!-- / #container -->

		<div class="clear"></div>
	</div>
</body>
