<body class="cl_body_hidden">

	<div id="ca_main">
		<script src="AMPOV0060.js"></script>

		<div id="container">

			<div id="mainColumn">
				<div id="mainColumninBox" class="noLeftColumn">

					<div id="content">
						<div class="fieldgroup" id="ca_base_form">
							<div class="fieldgroupInBox">
								<input type="hidden" id="ca_recno"> <input type="hidden"
									id="ca_state"> <input type="hidden" id="ca_id">
								<div class="fieldUnit mrgb10 flleft half mrgr20 required">
									<p class="fieldName">事業ユニット</p>
									<div class="fieldBox ca_unitID_div">
										<select class="mbn wt280 cl_valid cl_required" name="unitID"
											id="ca_unitID">
										</select>
									</div>
									<div class="clear"></div>
								</div>

								<div class="clear"></div>

								<!-- ＰＯ種別 -->
								<div class="fieldUnit mrgb10 half required">
									<p class="fieldName">ＰＯ種別</p>
									<div class="fieldBox ca_poTypeID_div">
										<select class="mbn wt280 cl_valid cl_required" name="poTypeID"
											id="ca_poTypeID">
										</select>
									</div>
									<div class="clear"></div>
								</div>

								<div class="clear"></div>

								<!-- ブランド -->
								<div class="fieldUnit mrgb10 half flleft  mrgr20 required">
									<p class="fieldName bl2">
										ブランド<br />コード
									</p>
									<div class="fieldBox">
										<span class="limit wt40 flright mrgl240" length="4"></span> <input
											type="text" placeholder=""
											class="form-control wt280 cl_valid cl_required" name="code"
											id="ca_code" data-tflimit="4" data-validator="maxlen:4 alnum"/>
									</div>
								</div>

								<div class="fieldUnit mrgb10 half flleft required">
									<p class="fieldName">ブランド名称</p>
									<div class="fieldBox">
										<span class="limit wt40 flright mrgl380" length="30"></span> <input
											type="text" placeholder=""
											class="form-control wt420  cl_valid cl_required" name="name"
											id="ca_name" data-tflimit="30" data-validator="maxlen:30"/>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>
								<!-- 親ブランド -->
								<div class="fieldUnit mrgb10 half  flleft mrgr20  required" id="ca_parentBrandID_div">
									<p class="fieldName">親ブランド</p>
									<div class="fieldBox ">
										<select class="mbn wt280 cl_valid cl_required" name="parentBrandID"
											id="ca_parentBrandID">
										</select>
									</div>
									<div class="clear"></div>
								</div>
								<!-- モデル -->
								<div class="fieldUnit mrgb10 half flleft required" id="ca_modelID_div">
									<p class="fieldName">モデル</p>
									<div class="fieldBox ">
										<select class="mbn wt280 cl_valid cl_required" name="modelID"
											id="ca_modelID">
										</select>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10  flleft mrgr20 required">
									<p class="fieldName">適用開始日</p>
									<div class="fieldBox">
										<div class="datepicker_wrap ca_stDate_div">
											<input type="text" id="ca_stDate" name="stDate"
												class="cl_date cl_valid cl_required ime-disabled"
												 data-validator="maxlen:13"/>
										</div>
									</div>
									<div class="clear"></div>
								</div>
								<div class="fieldUnit mrgb10  flleft mrgr20 required">
									<p class="fieldName">適用終了日</p>
									<div class="fieldBox">
										<div class="datepicker_wrap ca_edDate_div">
											<input type="text" id="ca_edDate" name="edDate"
												class="cl_date cl_valid cl_required ime-disabled"
												 data-validator="maxlen:13"/>
										</div>
									</div>
									<div class="clear"></div>
								</div>
								<div class="fieldUnit mrgb10  flleft mrgr20">
									<p class="fieldName">発注停止日</p>
									<div class="fieldBox">
										<!-- ツールチップでヘルプを表示 -->
										<p class="txtInFieldUnit flright help" id="ca_tp_code"
											title="発注停止日：この日付以降店舗ではそのマスタを参照不可になります。<br />適用終了日：店舗だけでなく本部でもそのマスタを参照不可になります。"><span>?</span></p>
										<div class="datepicker_wrap ca_ordStopDate_div">
											<input type="text" id="ca_ordStopDate" name="ordStopDate"
												class="cl_date cl_valid  ime-disabled"  data-validator="maxlen:13"/>
										</div>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>
							</div>
						</div>
						
						
						<!-- 2015/10/26 PO改善追加項目 藤岡 -->
						<div class="fieldgroup" id="ca_baseoptArea">
							<p class="gtitle">基本オプション</p>
							
							<div class="fieldgroupInBox">
								
								<div id="ca_AOKI_M_Field">
									<p class="fieldName"></p>
									<div class="fieldBox flleft ca_optArea ca_AOKI_M_Area">
										<ul>
											<li>
												<label class="checkbox ib flleft" for="ca_AOKI_M_fAmf">無料AMFステッチ
													<input type="checkbox" value="" id="ca_AOKI_M_fAmf" data-toggle="checkbox">
												</label>
											</li>
											<!-- 2015/11/11 MT-949対応
											<li>
												<label class="checkbox ib flleft" for="ca_AOKI_M_fLining">裏仕様
													<input type="checkbox" value="" id="ca_AOKI_M_fLining" data-toggle="checkbox">
												</label>
											</li>
											-->
											<li>
												<label class="checkbox ib flleft" for="ca_AOKI_M_fPocket">腰ポケット
													<input type="checkbox" value="" id="ca_AOKI_M_fPocket" data-toggle="checkbox">
												</label>
											</li>
											<li>
												<label class="checkbox ib flleft" for="ca_AOKI_M_fButton">重ねボタン
													<input type="checkbox" value="" id="ca_AOKI_M_fButton" data-toggle="checkbox">
												</label>
											</li>
											<li>
												<label class="checkbox ib flleft" for="ca_AOKI_M_fTrunkAdj">ジャケット中胴補正
													<input type="checkbox" value="" id="ca_AOKI_M_fTrunkAdj" data-toggle="checkbox">
												</label>
											</li>
											<li>
												<label class="checkbox ib flleft" for="ca_AOKI_M_fVestTrunkAdj">ベスト中胴補正
													<input type="checkbox" value="" id="ca_AOKI_M_fVestTrunkAdj" data-toggle="checkbox">
												</label>
											</li>
										</ul>
									</div>
									<div class="clear"></div>
									<!-- 2015/11/11 MT-949対応 -->
									<p class="fieldName ca_optArea ca_AOKI_M_Area"></p>
									<div class="fieldBox flleft ca_optArea ca_AOKI_M_Area">
										<ul>
											<li class="mrgr20 mrgt12 liningLabel"><span class="">裏仕様</span></li>
											<li><label class="radio">
												<input type="radio" name="ca_AOKI_M_fLining" value="1" data-toggle="radio"checked>総裏・背抜き・背抜き観音
											</label></li>
											<li><label class="radio">
												<input type="radio" name="ca_AOKI_M_fLining" value="2" data-toggle="radio">総裏・背抜き
											</label></li>
										</ul>
									</div>
									<div class="clear"></div>
									<!-- 2015/11/11 MT-949対応 ここまで-->
								</div>
								<div class="clear"></div>
								<div class="fieldBox flleft ca_optArea" id="ca_ORIHICA_M_Area">
									<ul>
										<li>
											<label class="checkbox ib flleft" for="ca_ORIHICA_M_fLining">裏仕様
												<input type="checkbox" value="" id="ca_ORIHICA_M_fLining" data-toggle="checkbox">
											</label>
										</li>
										<li>
											<label class="checkbox ib flleft" for="ca_ORIHICA_M_fPocket">腰ポケット
												<input type="checkbox" value="" id="ca_ORIHICA_M_fPocket" data-toggle="checkbox">
											</label>
										</li>
										<li>
											<label class="checkbox ib flleft" for="ca_ORIHICA_M_fButton">重ねボタン
												<input type="checkbox" value="" id="ca_ORIHICA_M_fButton" data-toggle="checkbox">
											</label>
										</li>
									</ul>
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
						</div>
