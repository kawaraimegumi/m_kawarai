<div id="ca_MDCMV0200_main">
<div id="mainColumn">
	<div id="mainColumninBox" class="mainColumninBox">
		<div id="title">
			<h2>表示項目</h2>
			<div class="titleInBoxRight">
			</div>
		</div>
		<!-- /#title -->




		<div id="content">
			<div class="fieldgroup">
				<div class="fieldgroupInBox search">
					<div class="fieldUnit">
						<div class="fieldBox noName">
							<select name="info" class="mbn wt280 flleft" id="ca_MDCMV0200_dispgroup">
							<option value="0">売上</option>
							<option value="1">項目2</option>
							<option value="2">項目3</option>
							</select>
						</div>
						<div class="clear"></div>
					</div>
				</div>
			</div>

			<div class="fieldgroup">
				<div class="fieldgroupInBox">
					<div class="table fieldUnit mrgb20 mrgr20 wt240 flleft hta">
						<div class="ht340 wt240 yScroll">
							<table class="hilightRow drilldown selectable" id="ca_MDCMV0200_tbl">
								<tbody id="ca_MDCMV0200_tbody">
								</tbody>
							</table>
						</div>
					</div>

					<script id="ca_MDCMV0200_tbody_tmp" type="text/x-jquery-tmpl">
					<tr id="${dispitem_id}" class="ca_MDCMV0200_dispitem">
						<td>${name}</td>
						<input type="hidden" name="dispitem_id" value="${dispitem_id}">
						<input type="hidden" name="name" value="${name}">
						<input type="hidden" name="sum_bitset" value="${sum_bitset}">
					</tr>
					</script>

					<div class="fieldUnit mrgb20 flleft hta wt280">
						<div class="fieldBox noName ht340 wt280 btnListbuilder">
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_VAL" gsanp-AnaDispItemDefs="AMAN_DI_S_VAL">実績</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_LYVAL" gsanp-AnaDispItemDefs="AMAN_DI_S_LYVAL">前年実績</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_LYRT" gsanp-AnaDispItemDefs="AMAN_DI_S_LYRT">前年比</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_LYDF" gsanp-AnaDispItemDefs="AMAN_DI_S_LYDF">前年差</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_VSUMRT" gsanp-AnaDispItemDefs="AMAN_DI_S_VSUMRT">縦軸構成比</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_VACMRT" gsanp-AnaDispItemDefs="AMAN_DI_S_VACMRT">縦軸累計比</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_HSUMRT" gsanp-AnaDispItemDefs="AMAN_DI_S_HSUMRT">横軸構成比</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_HACMRT" gsanp-AnaDispItemDefs="AMAN_DI_S_HACMRT">横軸累計比</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_SUMRT" gsanp-AnaDispItemDefs="AMAN_DI_S_SUMRT">全体構成比</button>
							<button id="" class="btn btn-primary wt120 flleft mrgb10 mrgr10 ca_MDCMV0200_add"
								gsanp-AnaDefs="AMAN_DEFS_SUMBIT_PLANRT" gsanp-AnaDispItemDefs="AMAN_DI_S_PLANRT">計画比</button>
						</div>
						<div class="clear"></div>
					</div>

					<div class="table fieldUnit mrgb20 wt240 flleft hta listbuilder">
						<div class="control mrgb10">
							<p class="sortUp mrgr10" id="ca_MDCMV0200_up">上へ</p>
							<p class="sortDown mrgr10" id="ca_MDCMV0200_down">下へ</p>
							<p class="delete" id="ca_MDCMV0200_edit_delall">すべて削除</p>
							<div class="clear"></div>
						</div>
						<div class="ht320 wt240 yScroll">
							<table class="hilightRow drilldown selectable" id="ca_MDCMV0200_edit_tbl">
								<tbody id="ca_MDCMV0200_edit_tbody">
								</tbody>
							</table>
						</div>
					</div>

					<script id="ca_MDCMV0200_edit_tbody_tmp" type="text/x-jquery-tmpl">
					<tr id="${dispitem_id}" class="ca_MDCMV0200_dispitem deletable order">
						<td><span class="ca_MDCMV0200_sort asc"></span><span class="ca_MDCMV0200_sort dsc"></span>
							<span class="data ca_MDCMV0200_dispitem_name">${name}</span>
							<span class="btn-delete flright ca_MDCMV0200_edit_delete"></span></td>
						<input type="hidden" name="dispitem_id" value="${dispitem_id}">
						<input type="hidden" name="name" value="${name}">
					</tr>
					</script>
					<div class="clear"></div>
				</div>
			</div>

		</div>
		<!-- /#content -->


		<div id="mainColumnFooter" class="analytics">
			<p class="cancel"><a id="ca_MDCMV0200_cancel">キャンセル</a></p>
			<p class="apply mrgCancel"><a id="ca_MDCMV0200_commit">確定</a></p>
		</div>
		<!-- /#mainColumnFooter -->
	</div>
	<!-- /#mainColumninBox -->
	</div>
	<!-- /#mainColumn -->
</div>