			<div class="titleInBoxRight">
			</div>
		</div>
		<!-- /#title -->

		<div id="content">

			<div id="selected" class="ca_MDCMV1030_multi selected">
				<p class="title">選択した内容</p>
				<div class="control"><span class="delete" id="ca_MDCMV1030_edit_delall">すべて削除</span>
				<div class="clear"></div>
				</div>
				<div id="innerScroll">
				<ul id="ca_MDCMV1030_edit_tbl">
				</ul>
				</div>
			</div>
			<!-- /#selected -->

			<div class="fieldgroup">
				<div id="ca_MDCMV1030_searchArea">
					<div id="search" class="fieldgroupInBox search">
						<div class="fieldUnit mrgb10 half flleft mrgr20">
							<p class="fieldName">店舗コード</p>
							<div class="fieldBox">
								<input type="text" placeholder="" class="form-control wt280 ime-disabled" id="ca_MDCMV1030_code" data-limit="len:20 digit"/>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
						<div class="fieldUnit mrgb10">
							<p class="fieldName">店舗名</p>
							<div class="fieldBox">
								<input type="text" placeholder="" class="form-control wt280 ime-active" id="ca_MDCMV1030_name" data-limit="len:32"/>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10">
							<p class="fieldName">組織</p>
							<div class="fieldBox">
								<input type="hidden" id="ca_MDCMV1030_func_id" />
								<input type="hidden" id="ca_MDCMV1030_org_id" />
								<input class="form-control wt280 flleft" type="text" placeholder="" id="ca_MDCMV1030_orgname" disabled/>
								<button class="btn btn-default wt160 flleft mrgl10" id="ca_MDCMV1030_show_MDCMV0170">参照...</button>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10">
							<p class="fieldName">開店日</p>
							<div class="fieldBox">
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_open_iymd_from" type="text" placeholder=""
										class="flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
								<p class="txtInFieldUnit pdg flleft">〜</p>
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_open_iymd_to" type="text" placeholder=""
										class="flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10">
							<p class="fieldName">閉店日</p>
							<div class="fieldBox">
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_close_iymd_from" type="text" placeholder=""
										class="flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
								<p class="txtInFieldUnit pdg flleft">〜</p>
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_close_iymd_to" type="text" placeholder=""
										class="flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10">
							<p class="fieldName">組織区分</p>
							<div class="fieldBox" style="padding-top: 12px;" id="ca_MDCMV1030_orgkind_chkall">
								<p class="checkall">すべて選択</p>
								<p class="uncheckall dispn">すべて選択をはずす</p>
								<ul id="ca_MDCMV1030_orgkind">
									<!--
									<li><label class="checkbox"><input type="checkbox" value="1" data-toggle="checkbox">HD会社</label></li>
									<li><label class="checkbox"><input type="checkbox" value="2" data-toggle="checkbox">事業ユニット</label></li>
									<li><label class="checkbox"><input type="checkbox" value="3" data-toggle="checkbox">ゾーン</label></li>
									<li><label class="checkbox"><input type="checkbox" value="4" data-toggle="checkbox">エリア</label></li>
									-->
									<li><label class="checkbox"><input type="checkbox" value="5" data-toggle="checkbox">店舗</label></li>
									<li><label class="checkbox"><input type="checkbox" value="6" data-toggle="checkbox">倉庫</label></li>
									<li><label class="checkbox"><input type="checkbox" value="7" data-toggle="checkbox">本部部署</label></li>
									<!--
									<li><label class="checkbox"><input type="checkbox" value="99" data-toggle="checkbox">その他</label></li>
									-->
								</ul>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<!-- <p class="expandFieldUnits mrgb10"><span class="expand">もっと詳しく</span><span class="unexpand dispn">閉じる</span></p>
						<div class="fieldUnitsHidden dispn">
						<hr>
						<div class="fieldUnit mrgb10">
							<p class="fieldName">開店日</p>
							<div class="fieldBox">
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_open_iymd_from" type="text" placeholder=""
										class="form-control flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
								<p class="txtInFieldUnit pdg flleft">〜</p>
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_open_iymd_to" type="text" placeholder=""
										class="form-control flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
								<div class="clear"></div>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10">
							<p class="fieldName">閉店日</p>
							<div class="fieldBox">
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_close_iymd_from" type="text" placeholder=""
										class="form-control flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
								<p class="txtInFieldUnit pdg flleft">〜</p>
								<div class="datepicker_wrap">
									<input id="ca_MDCMV1030_close_iymd_to" type="text" placeholder=""
										class="form-control flleft cl_date cl_valid ime-disabled" data-limit="len:10">
								</div>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
						</div><!-- / .fieldUnitsHidden -->

						<div class="fieldUnit">
							<div class="fieldBox mrgt20 noName btnCenter">
								<button id="ca_MDCMV1030_search" class="btn btn-info wt280 mrgr20">検索</button>
								<button class="btn btn-default wt140" id="ca_MDCMV1030_clear">クリア</button>
							</div>
							<div class="clear"></div>
						</div>
					</div>
				</div>
				<button id="searchAgain" class="dispn btn">検索条件を再指定</button>
			</div>

		<div id="result" class="dispn">
			<div class="fieldgroup">
				<div class="fieldgroupInBox">

					<div class="pagination-wrapper">
						<div class="count" id="ca_MDCMV1030_pager_displaypanel1"></div>
						<div class="pagination">
							<ul id="ca_MDCMV1030_pager1">
							</ul>
						</div>
					</div>

					<div class="table fieldUnit hta">
					<table class="hilightRow" id="ca_MDCMV1030_tbl">
						<tr>
							<th width="40px">
								<label class="checkbox" for=""><input type="checkbox" data-toggle="checkbox" id="ca_MDCMV1030_chkall"></label>
							</th>
			<!-- thのcheckboxで表組内のすべてのチェックをオン／オフ -->
							<th width="180px" class="ca_th_sort" key="CODE"><span>店舗コード</span></th>
							<th width="" class="ca_th_sort" key="NAME"><span>店舗名</span></th>
						</tr>
						<tbody id="ca_MDCMV1030_tbody">
						</tbody>
					</table>
					<script id="ca_MDCMV1030_tbody_single_tmp" type="text/x-jquery-tmpl">
					<tr id="${store_id}">
						<td><label class="radio" for=""><input type="radio" data-toggle="radio" name="ca_MDCMV1030_chk"></label></td>
						<td>${code}</td>
						<td>${name}</td>
					</tr>
					</script>
					<script id="ca_MDCMV1030_tbody_multiple_tmp" type="text/x-jquery-tmpl">
					<tr id="${store_id}">
						<td><label class="checkbox" for=""><input type="checkbox" data-toggle="checkbox" name="ca_MDCMV1030_chk"></label></td>
						<td>${code}</td>
						<td>${name}</td>
					</tr>
					</script>
					</div>
					<div class="mrgb20"></div>

					<div class="pagination-wrapper">
						<div class="count" id="ca_MDCMV1030_pager_displaypanel2"></div>
						<div class="pagination">
							<ul id="ca_MDCMV1030_pager2">
							</ul>
						</div>
					</div>

					<div class="fieldUnit">
						<div class="fieldBox mrgt30 mrgb30 noName btnCenter">
							<button class="btn btn-primary wt280 ca_MDCMV1030_multi" id="ca_MDCMV1030_add">チェックしたものを選択内容に追加</button>
						</div>
						<div class="clear"></div>
					</div>
				</div>
			</div>

		</div>

		</div>
		<!-- /#content -->

		<div id="mainColumnFooter">
			<p class="cancel"><a id="ca_MDCMV1030_cancel">キャンセル</a></p>
			<p class="apply mrgCancel"><a id="ca_MDCMV1030_commit">確定</a></p>
		</div>
		<!-- /#mainColumnFooter -->
	</div>
	<!-- /#mainColumninBox -->



</div>
<!-- /#mainColumn -->
</div>
