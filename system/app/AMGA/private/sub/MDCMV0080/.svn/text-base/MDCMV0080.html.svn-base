<div id="ca_MDCMV0080_main">
<div id="mainColumn">
	<div id="mainColumninBox" class="mainColumninBox">
		<div id="title">
			<h2>表示設定</h2>
			<div class="titleInBoxRight">
			</div>
		</div>
		<div id="content">
			<div class="fieldgroup">
				<div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">表示項目並び</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="MDCMV0080_disp_way" value="1" data-toggle="radio">縦並び</label></li>
							<li><label class="radio"><input type="radio" name="MDCMV0080_disp_way" value="2" data-toggle="radio" checked>横並び</label></li>
							<li><label class="radio"><input type="radio" name="MDCMV0080_disp_way" value="3" data-toggle="radio" checked>横並び（表示項目集約）</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">合計表示</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="MDCMV0080_f_total" value="1" data-toggle="radio" checked>あり</label></li>
							<li><label class="radio"><input type="radio" name="MDCMV0080_f_total" value="0" data-toggle="radio">なし</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">小計表示</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="MDCMV0080_f_subtotal" value="1" data-toggle="radio" checked>あり</label></li>
							<li><label class="radio"><input type="radio" name="MDCMV0080_f_subtotal" value="2" data-toggle="radio">なし</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">表示単位</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="MDCMV0080_disp_amunit" value="1" data-toggle="radio" checked>円単位</label></li>
							<li><label class="radio"><input type="radio" name="MDCMV0080_disp_amunit" value="2" data-toggle="radio">千単位</label></li>
							<li><label class="radio"><input type="radio" name="MDCMV0080_disp_amunit" value="3" data-toggle="radio">万単位</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">サイズ名で集約</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="MDCMV0080_sizesum" value="1" data-toggle="radio" checked>あり</label></li>
							<li><label class="radio"><input type="radio" name="MDCMV0080_sizesum" value="2" data-toggle="radio">なし</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div id="mainColumnFooter" class="analytics">
			<p class="cancel"><a id="MDCMV0080_cancel_button">キャンセル</a></p>
			<p class="apply mrgCancel"><a id="MDCMV0080_commit_button">確定</a></p>
		</div>
	</div>
</div>
</div>
