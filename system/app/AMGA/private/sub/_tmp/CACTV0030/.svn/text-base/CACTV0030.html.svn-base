<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="viewport" content="width=1024, initial-scale=1.0, maximum-scale=1.0" >
<title>分析カタログ編集</title>

<link href="../../../css/bootstrap.css" rel="stylesheet">
<link href="../../../css/prettify.css" rel="stylesheet">
<link href="../../../css/jquery-ui-1.10.3.custom.css" rel="stylesheet">

<link media="screen" rel="stylesheet" type="text/css" href="../../css/style.css">

<script src="../../../js/jquery-1.8.3.min.js"></script>
<script src="../../../js/jquery-ui-1.10.3.custom.min.js"></script>
<script src="../../../js/jquery.ui.touch-punch.min.js"></script>

<!-- Loading ui-folder -->
<script type="text/javascript" src="../../js/ui-folder.js"></script>

<script type="text/javascript" src="../../../js/jquery.easing-1.3.min.js"></script>
<script type="text/javascript" src="../../../js/jquery.blockUI.js"></script>
<script type="text/javascript" src="../../../js/underscore.js"></script>
<script type="text/javascript" src="../../../js/backbone.js"></script>
<script type="text/javascript" src="../../../js/json2.js"></script>
<script type="text/javascript" src="../../../js/jquery.tmpl.min.js"></script>
<script type="text/javascript" src="../../../js/jquery.exchangeselect.0.1.0.js"></script>

<script type="text/javascript" src="../../../js/backbone.syphon.js"></script>
<script type="text/javascript" src="../../../js/jquery.fixedheadertable.js"></script>
<script type="text/javascript" src="../../../js/jquery.skOuterClick.js"></script>
<!-- datepicker -->
<script type="text/javascript" src="../../../js/jquery-ui.js"></script>
<script type="text/javascript" src="../../../js/jquery.ui.datepicker-ja.min.js"></script>
<script type="text/javascript" src="../../../js/jquery.form.js"></script>

<!-- App -->
<script type="text/javascript" src="../../js/clcom.js"></script>
<script type="text/javascript" src="../CACTV0040/CACTV0040.js"></script>
<script type="text/javascript" src="CACTV0030.js"></script>

</head>

<body id="ca_body">

<div id="cl_header"></div><!-- /#header -->
<div class="cl_echoback msgBox error"></div>

<div id="ca_CACTV0040_dialog" class="cl_dialog"></div>
<div id="cl_dialog_area" class="cl_dialog"></div>

<div id="ca_main">
<div id="container">

	<div id="mainColumn">
	<div id="mainColumninBox" class="noLeftColumn">
		<div id="title">
			<h2>分析カタログ編集</h2>
			<div class="titleInBoxRight">
			</div>
		</div>
		<!-- /#title -->

		<div id="content">

			<div class="fieldgroup">
				<div class="fieldgroupInBox">
					<div class="ca_base_form">
						<div class="fieldUnit mrgb10 required">
							<p class="fieldName bl2">分析<br/>カタログ名</p>
							<div class="fieldBox">
								<input type="text" placeholder="" class="form-control wt280 ime-active cl_valid cl_required" data-limit="len:32" id="ca_name"/>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10 required">
							<p class="fieldName">公開区分</p>
							<div class="fieldBox" id="ca_f_open_div">
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10 ca_editBtn">
							<p class="fieldName">場所</p>
							<div class="fieldBox">
								<input type="hidden" id="ca_anamenunode_id">
								<input class="form-control wt280 flleft" type="text" id="ca_menutree" placeholder="" readonly />
								<button class="btn btn-default wt140 flleft mrgl10" id="ca_CACTV0040_show">参照...</button>
								<button class="btn btn-default wt140 flleft mrgl10" id="ca_menutree_clear">クリア</button>
							</div>
							<div class="clear"></div>
						</div>
						<input type="hidden" id="ca_id">
						<input type="hidden" id="ca_updcnt">
						<input type="hidden" id="ca_func_id">
						<input type="hidden" id="ca_anakind">
					</div>
					<div class="fieldUnit mrgb10 ca_period_area">
						<p class="fieldName">期間</p>
						<div class="fieldBox">
							<select name="info" class="mbn wt280 mrgb10" id="ca_ymd">
								<option class="ca_month" value="2" unit="月">月単位</option>
								<option class="ca_week" value="9" unit="週">週単位</option>
								<option class="ca_day" value="3" unit="日">日単位</option>
          				</select>
						</div>
						<div class="clear"></div>
					</div>
					<!-- slectによって以下に表示する内容を変える -->
					<div class="fieldUnit mrgb10 ca_period_area">
						<div class="fieldBox">
          				<ul>
							<li style="float: inherit;"><label class="radio flleft mrgr0"><input type="radio" name="ca_catalog_period" value="1" data-toggle="radio" checked></label>
								<div class="ca_type_before_div">
									<p class="txtInFieldUnit pdg flleft">当<span class="ca_unit"></span>の</p>
									<input type="text" placeholder="" class="form-control wt80 flleft cl_valid ime-disabled"
										data-limit="len:2 digit" data-required3="group2" id="ca_period_val1"/>
									<p class="txtInFieldUnit pdg flleft"><span class="ca_unit"></span>前から</p>
									<input type="text" placeholder="" class="form-control wt80 flleft cl_valid ime-disabled"
										data-limit="len:2 digit"data-required3="group2" id="ca_period_val2"/>
									<p class="txtInFieldUnit pdg flleft"><span class="ca_unit"></span>前まで</p>
								</div>
							</li>
						</ul>
						<div class="clear"></div>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb20 ca_period_area">
						<div class="fieldBox">
							<ul>
								<li style="float: inherit;">
									<label class="radio flleft mrgr0">
									<input type="radio" name="ca_catalog_period" value="0" data-toggle="radio"></label>

									<div id="ca_month_temp" class="ca_unit_ymd_div dispn">
										<select class="flleft ca_opt wt140 mrgl10 cl_valid"
												id="ca_month_y_period_val1"></select>
										<select class="flleft ca_opt wt140 mrgl10 cl_valid"
												id="ca_month_m_period_val1"></select>
										<p class="txtInFieldUnit pdg flleft">〜</p>
										<select class="flleft ca_opt wt140 cl_valid"
												id="ca_month_y_period_val2"></select>
										<select class="flleft ca_opt wt140 mrgl10 cl_valid"
												id="ca_month_m_period_val2"></select>
									</div>
									<div id="ca_week_temp" class="ca_unit_ymd_div dispn">
										<select class="flleft ca_opt wt140 mrgl10 cl_valid"
												id="ca_week_y_period_val1"></select>
										<div id="ca_week_w_period_val1_div"></div>
										<p class="txtInFieldUnit pdg flleft">〜</p>
										<select class="flleft ca_opt wt140 cl_valid"
												id="ca_week_y_period_val2"></select>
										<div id="ca_week_w_period_val2_div"></div>
									</div>
									<div id="ca_day_temp" class="ca_unit_ymd_div dispn">
										<div class="datepicker_wrap">
											<input type="text" class="ca_opt mrgl10 cl_date cl_valid ime-disabled"
												data-required2="group1" data-limit="len:10"
												id="ca_day_period_val1">
										</div>
										<p class="txtInFieldUnit pdg flleft">〜</p>
										<div class="datepicker_wrap">
											<input type="text" placeholder="" class="ca_opt cl_date cl_valid ime-disabled"
												data-required2="group1" data-limit="len:10"
												id="ca_day_period_val2">
										</div>
									</div>

								</li>
							</ul>
						</div>
					</div>
					<div class="clear"></div>

					<hr>

					<div class="fieldUnit hta ca_base_form">
						<p class="fieldName">ガイダンス</p>
						<div class="fieldBox">
							<textarea name="" id="ca_guide" rows="5" data-limit="len:150" class="form-control wt100pct cl_valid"></textarea>
						</div>
						<div class="clear"></div>
					</div>

				</div>
			</div>

			<div class="fieldgroup">
				<p class="gtitle">分析条件の変更を制限</p>
				<div class="fieldgroupInBox" id="ca_f_anacond_form">
					<div class="fieldUnit mrgb10">
						<p class="fieldName">期間</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_PERIOD" data-toggle="checkbox">制限する</label>
						</div>
					</div>
					<div class="fieldUnit mrgb10">
						<p class="fieldName">軸</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_AXIS" data-toggle="checkbox">制限する</label>
						</div>
					</div>
					<div class="fieldUnit mrgb10">
						<p class="fieldName">表示項目</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_DISP" data-toggle="checkbox">制限する</label>
						</div>
					</div>
					<div class="fieldUnit mrgb10">
						<p class="fieldName">店舗</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_STORE" data-toggle="checkbox">制限する</label>
						</div>
					</div>
					<div class="fieldUnit mrgb10">
						<p class="fieldName bl2">商品・<br/>商品分類</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_ITEM" data-toggle="checkbox">制限する</label>
						</div>
					</div>
					<div class="fieldUnit mrgb10">
						<p class="fieldName">会員</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_MEMB" data-toggle="checkbox">制限する</label>
						</div>
					</div>
					<div class="fieldUnit mrgb10">
						<p class="fieldName">社員</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_STAFF" data-toggle="checkbox">制限する</label>
						</div>
					</div>
					<div class="fieldUnit mrgb10">
						<p class="fieldName">売上</p>
						<div class="fieldBox">
							<label class="checkbox ib wt100" for=""><input type="checkbox" value="" id="ca_chk_SALE" data-toggle="checkbox">制限する</label>
						</div>
					</div>
				</div>
			</div>


		</div>
		<!-- /#content -->

		<div id="mainColumnFooter" class="noLeftColumn">
			<p class="cancel"><a id="ca_cancel">キャンセル</a></p>
			<p class="apply mrgCancel"><a id="ca_commit">保存</a></p>
		</div>
		<!-- /#mainColumnFooter -->

	</div>
	<!-- /#mainColumninBox -->

	</div>
	<!-- /#mainColumn -->
</div><!-- / #container -->
</div><!-- /#ca_main -->
<div class="clear"></div>

</body>
</html>
