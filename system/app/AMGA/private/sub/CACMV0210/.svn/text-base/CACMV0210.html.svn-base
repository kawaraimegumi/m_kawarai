<div id="ca_CACMV0210_main">
<div id="mainColumn">
	<div id="mainColumninBox" class="mainColumninBox">
		<div id="title">
			<h2>表示設定</h2>
			<div class="titleInBoxRight">
			</div>
		</div>
		<!-- /#title -->

		<div id="content">

			<div class="fieldgroup" id="ca_CACMV0210_searchArea">
				<div class="fieldgroupInBox" id="ca_CACMV0210_disp_way">
					<div class="fieldUnit">
						<p class="fieldName">表示項目並び</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_way" value="1" data-toggle="radio">縦並び</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_way" value="2" data-toggle="radio" checked>横並び</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_way" value="3" data-toggle="radio" checked>横並び（表示項目集約）</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">合計表示</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_f_total" value="1" data-toggle="radio" checked>あり</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_f_total" value="0" data-toggle="radio">なし</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox" id="ca_CACMV0210_f_subtotal">
					<div class="fieldUnit">
						<p class="fieldName">小計表示</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_f_subtotal" value="1" data-toggle="radio" checked>あり</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_f_subtotal" value="2" data-toggle="radio">なし</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox" id="ca_CACMV0210_existsum">
					<div class="fieldUnit">
						<p class="fieldName">既存店集計</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_existsum" value="1" data-toggle="radio">する</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_existsum" value="2" data-toggle="radio" checked>しない</label></li>

							<li class="fieldName mrgl20" style="position:static;">既存店基準日</li>
							<li><div class="datepicker_wrap">
								<input id="ca_CACMV0210_exist_iymd" class="w120 cl_date cl_required ime-disabled" type="text" data-limit="len:10" maxlength="10" size="10">
							</div></li>
						</ul>
						</div>
					</div>
				</div>
				<div class="clear"></div>
				<div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">表示単位</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_amunit" value="1" data-toggle="radio" checked>円単位</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_amunit" value="2" data-toggle="radio">千単位</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_amunit" value="3" data-toggle="radio">万単位</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<div class="fieldgroupInBox" id="ca_CACMV0210_sizesum">
					<div class="fieldUnit">
						<p class="fieldName">サイズ名で集約</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_sizesum" value="1" data-toggle="radio">あり</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_sizesum" value="2" data-toggle="radio" checked>なし</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div>
				<!-- <div class="fieldgroupInBox">
					<div class="fieldUnit">
						<p class="fieldName">昨年</p>
						<div class="fieldBox">
						<ul>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_ly" value="1" data-toggle="radio" checked>昨年同日</label></li>
							<li><label class="radio"><input type="radio" name="ca_CACMV0210_disp_ly" value="2" data-toggle="radio">昨年同曜日</label></li>
						</ul>
						<div class="clear"></div>
						</div>
					</div>
				</div> -->
			</div>

		</div>
		<!-- /#content -->


		<div id="mainColumnFooter" class="analytics">
			<p class="cancel"><a id="ca_CACMV0210_cancel">キャンセル</a></p>
			<p class="apply mrgCancel"><a id="ca_CACMV0210_commit">確定</a></p>
		</div>
		<!-- /#mainColumnFooter -->

	</div>
	<!-- /#mainColumninBox -->
	</div>
	<!-- /#mainColumn -->
</div>
