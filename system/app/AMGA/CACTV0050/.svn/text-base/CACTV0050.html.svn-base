<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="viewport" content="width=1024, initial-scale=1.0, maximum-scale=1.0" >
<title>スケジュール登録</title>

<link href="../../AMGA/public/css/bootstrap.css" rel="stylesheet">
<link href="../../AMGA/public/css/prettify.css" rel="stylesheet">

<link media="screen" rel="stylesheet" type="text/css" href="../../AMGA/private/css/style.css">
<link media="screen" rel="stylesheet" type="text/css" href="../../AMGA/private/css/style-AMGAV.css">
<link href="../public/css/jquery-ui-1.10.3.custom.css" rel="stylesheet">

<script src="../../AMGA/public/js/jquery-1.8.3.min.js"></script>
<script src="../../AMGA/public/js/jquery-ui-1.10.3.custom.min.js"></script>
<script src="../../AMGA/public/js/jquery.ui.touch-punch.min.js"></script>

<!-- Loading ui-folder -->
<script type="text/javascript" src="../../AMGA/private/js/ui-folder.js"></script>

<script type="text/javascript" src="../../AMGA/public/js/jquery.easing-1.3.min.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/jquery.blockUI.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/underscore.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/backbone.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/json2.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/jquery.tmpl.min.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/jquery.exchangeselect.0.1.0.js"></script>

<script type="text/javascript" src="../../AMGA/public/js/backbone.syphon.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/jquery.fixedheadertable.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/jquery.skOuterClick.js"></script>
<!-- datepicker -->
<script type="text/javascript" src="../../AMGA/public/js/jquery-ui.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/jquery.ui.datepicker-ja.min.js"></script>
<script type="text/javascript" src="../../AMGA/public/js/jquery.form.js"></script>

<!-- App -->
<script type="text/javascript" src="../../AMGA/private/js/aman.js"></script>
<script type="text/javascript" src="../../AMGA/private/js/clcom-ana.js"></script>
<script type="text/javascript" src="../../AMGA/private/js/messages2-ana_ja.js"></script>
<script type="text/javascript" src="../../AMGA/private/js/clcom-ana-fix.js"></script>
<script type="text/javascript" src="../../AMGA/private/js/ana-testdata.js"></script>

<script type="text/javascript" src="../../AMGA/private/sub/CACTV0040/CACTV0040.js"></script>
<link media="screen" rel="stylesheet" type="text/css" href="../private/css/style-AMGAV.css">
<script type="text/javascript" src="CACTV0050.js"></script>

</head>

<body id="ca_body">

<div id="cl_dialog_area" class="cl_dialog"></div>
<div id="page-wrap">
<div id="cl_header"></div><!-- /#header -->
<div class="cl_echoback msgBox error"></div>

<div id="ca_main">
<div id="container">

	<div id="mainColumn">
	<div id="mainColumninBox" class="noLeftColumn">
		<div id="title">
			<h2>スケジュール登録</h2>
			<div class="titleInBoxRight">
			</div>
		</div>
		<!-- /#title -->

		<div id="content">

			<div class="fieldgroup">
				<div class="fieldgroupInBox view ca_base_form">
					<div class="fieldUnit mrgb20 half flleft">
						<p class="fieldName">分析カタログ名</p>
						<div class="fieldBox">
							<p class="txtbox flleft wt380" id="ca_anacatlog_name"></p>
						</div>
					</div>
					<div class="clear"></div>

					<div class="js--addcontrol1--wrap">
						<div id="div_addcontrol">
						</div>

						<div class="row">
							<div class="col-md-12 form-group addcontrol">
								<button id="ca_btn_add" class="btn btn-flat-default js--addcontrol1"><span class="icon-abui-plus prx"></span>スケジュールを追加</button>
							</div>
						</div>
						<!-- テンプレート -->
						<script id="ca_template_addcontrol" type="text/template">
							<div class="row js--addcontrol1" number="<%- seqno %>">
								<input type="hidden" name="ca_id" value="<%- id %>" />
								<input type="hidden" name="ca_updcnt" value="<%- updcnt %>" />
								<div class="wt180 flleft mrgl10">
									<div class="form-group required">
										<label class="label--code mrgb10">サイクル<%- seqno %></label>
										<select name="ca_cycle_typeid" class="wt180 cl_valid">
											<option value="1" selected>毎日</option>
											<option value="2">毎週</option>
											<option value="3">毎月</option>
											<option value="4">特定日付</option>
										</select>
									</div>
								</div>
								<div class="wt180 flleft mrgl20">
									<div class="wt180 form-group required" name="div_tgtymd">
										<label class="label--name mrgb10">出力日<%- seqno %></label>
										<div name="div_tgtymd_1">
											<input type="text" class="form-control" name="ca_tgtymd_1" disabled />
										</div>
										<div name="div_tgtymd_2" class="dispn">
											<select name="ca_tgtymd_2" class="wt180" multiple>
												<option value="2">月</option>
												<option value="3">火</option>
												<option value="4">水</option>
												<option value="5">木</option>
												<option value="6">金</option>
												<option value="7">土</option>
												<option value="1">日</option>
											</select>
										</div>
										<div name="div_tgtymd_3" class="dispn">
											<select name="ca_tgtymd_3" class="wt180" multiple>
												<option value="1">1</option>
												<option value="2">2</option>
												<option value="3">3</option>
												<option value="4">4</option>
												<option value="5">5</option>
												<option value="6">6</option>
												<option value="7">7</option>
												<option value="8">8</option>
												<option value="9">9</option>
												<option value="10">10</option>
												<option value="11">11</option>
												<option value="12">12</option>
												<option value="13">13</option>
												<option value="14">14</option>
												<option value="15">15</option>
												<option value="16">16</option>
												<option value="17">17</option>
												<option value="18">18</option>
												<option value="19">19</option>
												<option value="20">20</option>
												<option value="21">21</option>
												<option value="22">22</option>
												<option value="23">23</option>
												<option value="24">24</option>
												<option value="25">25</option>
												<option value="26">26</option>
												<option value="27">27</option>
												<option value="28">28</option>
												<option value="99">月末</option>
											</select>
										</div>
										<div name="div_tgtymd_4" class="dispn">
											<div class="datepicker_wrap"><input type="text" name="ca_tgtymd_4" class="cl_date cl_valid" /></div>
										</div>
									</div>
								</div>
								<div class="wt180 flleft mrgl20 fieldUnit">
									<div class="form-group">
										<label for="code1" class="label--last mrgb10">出力ファイル<%- seqno %></label>
										<ul>
											<li class="flleft"><label class="radio"><input type="radio" name="ca_output_typeid<%- seqno %>" namex="ca_output_typeid" value="1" data-toggle="radio"  />Excel</label></li>
											<li class="flleft"><label class="radio"><input type="radio" name="ca_output_typeid<%- seqno %>" namex="ca_output_typeid" value="2" data-toggle="radio"  />CSV</label></li>
										</ul>
									</div>
								</div>
								<div class="wt140 flleft mrgl20 fieldUnit">
									<div class="form-group">
										<label for="code1" class="label--last mrgb10">最終実行日<%- seqno %></label>
										<p class="txtbox wt100"><%- clutil.dateFormat(last_exec_date, "yyyy/mm/dd") %></p>
									</div>
								</div>
								<div class="wt140 flleft mrgl10 fieldUnit">
									<div class="form-group">
										<label for="code1" class="label--next mrgb10">次回実行予定日<%- seqno %></label>
										<p class="txtbox wt100"><%- clutil.dateFormat(next_exec_date, "yyyy/mm/dd") %></p>
									</div>
								</div>
								<div class=" flleft mrgl10 mrgt13">
									<div class="delFieldUnits" name="del_row_sch">
										<!-- 削除ボタン -->
										<span></span>
									</div>
								</div>
							</div>
						</script>
					</div>
				</div>
			</div>


		</div>
		<!-- /#content -->

		<div id="mainColumnFooter" class="noLeftColumn">
			<p class="cancel"><a id="ca_cancel">キャンセル</a></p>
			<p class="apply mrgCancel"><a id="ca_commit">保存</a></p>
		</div>
		<!-- /#mainColumnFooter -->

	</div>
	<!-- /#mainColumninBox -->

	</div>
	<!-- /#mainColumn -->
</div><!-- / #container -->
</div><!-- /#ca_main -->
<div class="clear"></div>
</div>

</body>
</html>
