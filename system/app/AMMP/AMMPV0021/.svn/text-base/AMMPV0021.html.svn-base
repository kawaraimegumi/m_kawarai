<script id="VersionView" type="text/template">
    <div class="modal-header">
        <h4 class="modal-title"><%- it.title %></h4>
    </div>
    <div class="clear"></div>
    <div class="table versionTable">
        <div class="table-header">
	        <table class="table mrgt10 hilightRow">
                <thead>
                    <th class="col1">バージョン</th>
                    <th class="col2">更新日時</th>
                    <th class="col3">&nbsp;</th>
                </thead>
            </table>
        </div>
        <div class="table-body">
            <table class="table">
                <tbody>
                    <% it.collection.each(function(model){ %>
	                <tr id="<%- model.cid %>">
                        <% var data = model.attributes; %>
                        <td class="col1"><%- data.version %></td>
                        <td class="col2"><%- clutil.dateFormat(data.updDate, 'yyyy/mm/dd') %>　<%- clutil.timeFormat(data.updTime, 'hh:mm') %></td>
                        <td class="col3">
                            <a href="javascript:void(0);" class="planLink">ダウンロード</a>
                        </td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>        
</script>

<script id="DlUpView1" type="text/template">
    <hr/>
    <div class="table dlUpView1">
        <table class="table">
            <thead>
                <tr>
                    <th class="col1">&nbsp;</th>
                    <th class="col2">バージョン</th>
                    <th class="col3">更新日時</th>
                    <th class="col4">&nbsp;</th>
                    <th class="col5">&nbsp;</th>
                </tr>
            </thead>
            <tbody>
                <% if (it.planAuto.version) { %>
                <tr>
                    <td class="col1">自動按分計画</td>
                    <td class="col2"><%- it.planAuto.version %></td>
                    <td class="col3"><%- clutil.dateFormat(it.planAuto.updDate, 'yyyy/mm/dd') %>　<%- clutil.timeFormat(it.planAuto.updTime, 'hh:mm') %></td>
                    <td class="col4">
                        <a href="javascript:void(0);" class="planAutoLink">ダウンロード</a>
                    </td>
                    <td class="col5">
                        <a href="javascript:void(0);" class="planAutoMore">他バージョンを参照</a>
                    </td>
                </tr>
                <% } %>
                <% if (it.planInput.version) { %>
                <tr>
                    <td class="col1">取込計画</td>
                    <td class="col2"><%- it.planInput.version %></td>
                    <td class="col3"><%- clutil.dateFormat(it.planInput.updDate, 'yyyy/mm/dd') %>　<%- clutil.timeFormat(it.planInput.updTime, 'hh:mm') %></td>
                    <td class="col4">
                        <a href="javascript:void(0);" class="planInputLink">ダウンロード</a>
                    </td>
                    <td class="col5">
                        <a href="javascript:void(0);" class="planInputMore">他バージョンを参照</a>
                    </td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </div>
    <div class="clear"></div>
    <hr/>
	<div class="fieldUnits" id="season_add_info">
		<div class="fieldUnit mrgb20 ">
			<div class="fieldBox noName flright">
				<input type="file" class="hide-input" id="file_ul_season_plan"/>
				<button class="btn btn-default  mrgl10 wt320 ca_uptake" id="ca_ul_season_plan">計画をアップロード</button>
			</div>
			<div class="fieldBox noName flright">
				<button class="btn btn-default mrgl10 wt280 ca_sample_download">Excelサンプルダウンロード</button>
			</div>
			<div class="clear"></div>
		</div>
		<div class="clear"></div>
	</div>
</script>

