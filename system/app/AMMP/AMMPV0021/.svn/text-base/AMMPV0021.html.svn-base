					<select class="mbn wt280 cl_valid cl_required" id="ca_srchUnitID"></select>
				</div>
				<div class="clear"></div>
			</div>
			<div class="clear"></div>
			<div class="fieldUnit mrgb10 half flleft requiredSelect">
				<p class="fieldName">年度</p>
				<div class="fieldBox">
					<select class="mbn wt280 cl_valid cl_required" id="ca_srchYear"></select>
				</div>
				<div class="clear"></div>
			</div>
			<div class="clear"></div>
			<div class="fieldUnit mrgb10 flleft half mrgr10 required">
				<p class="fieldName">品種</p>
				<div class="fieldBox">
					<input type="text" class="form-control wt280 cl_valid cl_required" id="ca_srchItgrpID">
				</div>
				<div class="clear"></div>
			</div>
			<div class="clear"></div>

			<div class="fieldUnit mrgb10 flleft half mrgr10 required">
				<p class="fieldName">シーズン</p>
				<div class="fieldBox">
					<select class="mbn wt280 cl_valid cl_required" id="ca_srchSeasonID"></select>
				</div>
				<div class="clear"></div>
			</div>
			<div class="clear"></div>
            
			<div class="fieldUnit">
				<div class="fieldBox mrgt20 noName btnCenter">
					<button id="search" class="btn btn-info wt280">検索</button>
				</div>
				<div class="clear"></div>
			</div>
		</div>
		<button id="searchAgain" class="dispn btn">検索条件を再指定</button>
	</div>
</script>

