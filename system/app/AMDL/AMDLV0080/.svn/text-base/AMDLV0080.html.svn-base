<body class="cl_body_hidden">
<div id="ca_main">

<script src="AMDLV0080.js"></script>

<!-- 共通ヘッダは MDBaseView によって置き換えるので、設置不要。
<div id="header">
	<p class="back"></p>
	<h1 class="contextmenu">入出荷・返品</h1>
	<div class="rightBox">
		<span class="logo"></span>
		<span class="logout"><a href="../../../index.html">ログアウト</a></span>
		<span class="user">Username</span>

	</div>
</div><!-- /#header -->

<div id="container">
	<!-- 店舗選択補助画面 -->
	<div id="ca_AMPAV0010_dialog" class="cl_dialog"></div>
	<div id="ca_AMPAV0020_dialog" class="cl_dialog"></div>

	<div id="mainColumn">
	<div id="mainColumninBox" class="noLeftColumn">
		<!-- タイトル部は MDBaseView によって置き換えるので、設置不要。
		<div id="title">
			<h2>入荷予定リスト</h2>
			<div class="titleInBoxRight">
			</div>
		</div>
		<!-- /#title -->


		<div id="content">

			<div class="fieldgroup">
				<div id="ca_srchArea" class="fieldgroupInBox search">
					<div class="fieldUnit mrgb10 required"">
						<p class="fieldName">店舗</p>
						<div class="fieldBox id="div_store">
							<input class="form-control wt280 flleft cl_valid cl_required cl_store" type="text" placeholder="" id="ca_srchStoreID" />
							<button class="btn btn-default wt140 flleft mrgl10 ca_btn_store" id="ca_btn_store_select" tabindex="-1">参照...</button>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>

					<div class="fieldUnit mrgb10 half flleft">
						<p class="fieldName">伝票区分</p>
						<div class="fieldBox">
							<select class="mbn wt280" id="ca_srchSlipFmtID"></select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 flleft">
						<p class="fieldName">出荷元</p>
						<div class="fieldBox" id="div_vendor">
							<input id="ca_srchVendorID" type="text" placeholder="" class="form-control flleft wt280 cl_valid" />
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 flleft">
						<p class="fieldName">店舗・倉庫</p>
						<div class="fieldBox" id="div_vendor">
							<input id="ca_srchOutStoreID" type="text" placeholder="" class="form-control flleft wt280 cl_valid cl_store" />
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 mrgr20">
						<p class="fieldName bl2">出荷データ<br />登録日</p>
						<div class="fieldBox">
							<div class="datepicker_wrap"><input class="cl_date cl_valid" type="text" name="fromDate" id="ca_fromDate" /></div>
							<p class="txtInFieldUnit pdg flleft">〜</p>
							<div class="datepicker_wrap"><input class="cl_date cl_valid" type="text" name="toDate" id="ca_toDate" /></div>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half flleft mrgr20">
						<p class="fieldName">荷番</p>
						<div class="fieldBox">
							<input type="text" placeholder="" class="form-control wt280 cl_valid" name="srchShipNo" id="ca_srchShipNo" data-validator="maxlen:6 digit" />
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 mrgr20">
						<p class="fieldName">伝票番号</p>
						<div class="fieldBox">
							<input type="text" placeholder="" class="form-control wt280 cl_valid"  name="srchDlvNo" id="ca_srchDlvNo" data-validator="maxlen:8 digit" />
						</div>
						<div class="clear"></div>
					</div>


					<div class="fieldUnit">
						<div class="fieldBox mrgt20 noName btnCenter">
							<button class="btn btn-info wt280" id="ca_srch">検索</button>
						</div>
						<div class="clear"></div>
					</div>
				</div>
				<button id="searchAgain" class="dispn btn">検索条件を再指定</button>
			</div>

		<div id="result" class="dispn">

			<!-- ページネーション＃１：div.pagination-wrapper だけ設置してください -->
			<div class="pagination-wrapper"></div>

			<div class="table">
				<table class="hilightRow" id="ca_table">
				<thead>
					<tr>
						<!-- １番目の要素は全選択チェックボックス用に予約 -->
						<!--th width="40px" class="cl_checkbox_selectall">?全選択</th-->
						<!-- thのcheckboxで表組内のすべてのチェックをオン／オフ -->
						<th width="120px">部門</th>
						<th width="120px">品種</th>
                        <th width="120px">伝票区分</th>
						<th width="120px">出荷元</th>
						<th width="120px">荷番</th>
						<th width="120px">伝票番号</th>
						<th width="120px">出荷データ登録日</th>
						<th width="120px txtar">予定数</th>
					</tr>
				</thead>
				<tbody>
				</tbody>
				</table>
				<!-- 行テンプレート: underscore.js テンプレートを使用 -->
				<script id="ca_rec_template" type="text/template">
				<!--script id="ca_AMDLV0010_table_tbody_template" type="text/x-jquery-tmpl"-->
					<tr>
 					<!--tr class="csptr"-->
						<!--td class="cl_checkbox_selectrec">?行選択</td-->
						<td class="ca_c_link"><%- divDispName %></td>
						<td class="ca_c_link"><%- stditgrpDispName %></td>
                        <!-- 伝票区分 -->
						<td class="ca_c_link"><%- clutil.gettypename(amcm_type.AMCM_TYPE_SLIP_FORMAT, slipFmtID) %></td>
                        <!-- 出荷元 -->
						<td class="ca_c_link"><%- senderName %></td>
						<td class="ca_c_link"><%- shipNo %></td>
						<td class="ca_c_link"><%- dlvNo %></td>
						<td class="ca_c_link"><%- clutil.dateFormat(schedDate, 'yyyy/mm/dd(w)') %></td>
						<td class="ca_c_link txtar"><%- qy %></td>
					</tr>
				</script>
			</div>

			<!-- ページネーション＃１：div.pagination-wrapper だけ設置してください -->
			<div class="pagination-wrapper"></div>

			<!--div class="fieldUnit">
				<div class="fieldBox mrgt30 mrgb30 noName cl_opebtngroup" data-cl_groupid="AMDLV0080">
				<button class="btn btn-primary wt280 flleft mrgr10" id="cl_edit">編集</button>
				<button class="btn btn-default wt140 flright mrgl20 delete" id="cl_delete">削除</button>
			</div>
			<div class="clear"></div-->
		</div>


		</div>
		<!-- /#content -->


	</div>
	<!-- /#mainColumninBox -->



	</div>
	<!-- /#mainColumn -->


</div><!-- / #container -->

<div class="clear"></div>

<script src="js/jquery.contextmenu.js"></script>


</div>
</div>
</body>
</html>
