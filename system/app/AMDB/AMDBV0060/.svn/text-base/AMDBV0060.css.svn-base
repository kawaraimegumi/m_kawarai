.field {
  margin-bottom: 5px;
  margin-right: 15px;
}
._table {
  margin-top: 5px;
  overflow-x: scroll;
  overflow-y: scroll;
}
.rankName {
  text-align: center;
  font-family: Meiryo;
  font-style: normal;
  font-weight: bold;
  font-size: 10px;
  color: rgba(82, 94, 112, 1);
}
._rank {
  text-align: center;
  font-family: Meiryo;
  font-style: normal;
  font-weight: bold;
  font-size: 40px;
  color: rgba(82, 94, 112, 1);

  min-width: 150px;
  padding-top: 3px;
}
.rspRecColumnName {
  text-align: center;
  font-family: Meiryo;
  font-style: normal;
  font-weight: bold;
  font-size: 10px;
  color: rgba(130, 139, 151, 1);
}
.rspRecVal {
  text-align: center;
  font-family: Microsoft PhagsPa;
  font-style: normal;
  font-weight: bold;
  font-size: 19px;
  color: rgba(50, 58, 69, 1);

  min-width: 150px;
}
#request2 {
  font-weight: normal;
  padding-left: 5px;
}
.header {
  text-align: left;
  font-family: Meiryo;
  font-style: normal;
  font-weight: bold;
  font-size: 10px;
  color: rgba(82, 94, 112, 1);

  background-color: rgba(240, 241, 242, 1);
  height: 15px;
  padding: 3.5px;
}
th {
  z-index: 2;
}
th:first-child {
  background-color: rgba(255, 255, 255, 1);
  left: 0;
  position: sticky;
  z-index: 3;
}
td:first-child {
  background-color: rgba(255, 255, 255, 1);
  left: 0;
  position: sticky;
  z-index: 1;
}
.columnName1 {
  text-align: right;
  font-family: Meiryo;
  font-style: normal;
  font-weight: bold;
  font-size: 10px;
  color: rgba(130, 139, 151, 1);
}
.columnName2 {
  text-align: left;
  font-family: Meiryo;
  font-style: normal;
  font-weight: normal;
  font-size: 10px;
  color: rgba(130, 139, 151, 1);
}
.rank {
  text-align: right;
  font-family: Meiryo;
  font-style: normal;
  font-weight: bold;
  font-size: 10px;
  color: rgba(130, 139, 151, 1);

  padding-right: 5px;
  width: 45px;
}
.org {
  text-align: center;
  font-family: Meiryo;
  font-style: normal;
  font-weight: bold;
  font-size: 10px;
  color: rgba(130, 139, 151, 1);

  width: calc(100% - 45px);
}
.val {
  text-align: right;
  font-family: Microsoft PhagsPa;
  font-style: normal;
  font-weight: normal;
  font-size: 10px;
  color: rgba(50, 58, 69, 1);
}
._row {
  height: 15px;
}
.searchedOrgRow * {
  color: rgba(52, 152, 219, 1);
}

#sex .btn-group > .btn:last-of-type,
.btn-group
  > .btn:last-of-type:not(:first-child):not(:last-child):not(.dropdown-toggle) {
  width: 145px;
}
#age .btn-group > .btn:last-of-type,
.btn-group
  > .btn:last-of-type:not(:first-child):not(:last-child):not(.dropdown-toggle) {
  width: 145px;
}
#market .btn-group > .btn:last-of-type,
.btn-group
  > .btn:last-of-type:not(:first-child):not(:last-child):not(.dropdown-toggle) {
  width: 145px;
}
#newmemb .btn-group > .btn:last-of-type,
.btn-group
  > .btn:last-of-type:not(:first-child):not(:last-child):not(.dropdown-toggle) {
  width: 145px;
}
