<body class="cl_body_hidden">
<div id="ca_main">

<script src="AMBRV0020.js"></script>

<!-- 共通ヘッダは MDBaseView によって置き換えるので、設置不要。-->
<!-- エコーバックエリアは MDBaseView によって置き換えるので、設置不要。-->
<!-- ダイアログ設定：MDBaseView によって置き換えるので、設置不要。 -->

<div id="container">
	<!-- 店舗選択補助画面 -->
	<div id="ca_AMPAV0010_dialog" class="cl_dialog"></div>
	<div id="ca_AMPAV0020_dialog" class="cl_dialog"></div>
	<div id="ca_AMPAV0010_2_dialog" class="cl_dialog"></div>
	<div id="ca_AMPAV0020_2_dialog" class="cl_dialog"></div>

	<div id="mainColumn">
		<div id="mainColumninBox" class="noLeftColumn">
			<!-- タイトル部は MDBaseView によって置き換えるので、設置不要。-->

			<div id="content">
				<div class="fieldgroup">
					<div id="ca_srchArea" class="fieldgroupInBox search">
						<div class="fieldUnit mrgb10 flleft half mrgr20 required" id="ca_srchUnitID_div">
							<p class="fieldName">事業ユニット</p>
							<div class="fieldBox">
								<select class="mbn wt280 cl_valid cl_required" id="ca_srchUnitID" name="srchUnitID"></select>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<div class="fieldUnit mrgb10">
							<p class="fieldName">店舗</p>
							<div class="fieldBox">
								<input class="form-control wt280 flleft cl_valid cl_store" type="text" id="ca_srchOrgID"/>
								<button class="btn btn-default wt140 flleft mrgl10" id="ca_btn_org_select" tabindex="-1">参照...</button>
								<label class="checkbox ib mrgl10" for="check1"><input type="checkbox" value="" name="srchExistSw" id="ca_srchExistSw" data-toggle="checkbox">既存店のみ表示</label>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<div class="fieldUnit mrgb10 half flleft mrgr20 required">
							<p class="fieldName">品種</p>
							<div class="fieldBox">
								<input type="text" placeholder="" class="form-control wt280 cl_valid cl_required" id="ca_srchItgrpID"/>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<div class="fieldUnit mrgb10 half flleft mrgr20">
							<p class="fieldName">シーズン</p>
							<div class="fieldBox">
								<select multiple class="mbn wt280" name="srchSeasons" id="ca_srchSeasonSw"></select>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10 half flleft">
							<p class="fieldName">商品属性1</p>
							<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_srchItemAttr1Sw"></select>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<div class="fieldUnit mrgb10 half flleft mrgr20 required">
							<p class="fieldName bl2">プライス<br />集約単位</p>
							<div class="fieldBox">
								<!-- ツールチップでヘルプを表示 -->
								<p class="txtInFieldUnit flright help" id="ca_tp_price" title="値引き後の実売価で集約する場合は「1:販売価格」を、プライスラインで集約する場合は「2:指定上代」を、POS元上代で集約する場合は「3:POS元上代」を選択してください。"><span>?</span></p>
								<select name="info" class="mbn wt280 cl_valid cl_required" id="ca_srchPriceUnit"></select>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10 half flleft">
							<p class="fieldName">商品属性2</p>
							<div class="fieldBox">
								<select name="info" class="mbn wt280 cl_valid" id="ca_srchItemAttr2Sw"></select>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<div class="fieldUnit mrgb10 half flleft mrgr20">
							<p class="fieldName">マーケット</p>
							<div class="fieldBox">
								<select multiple class="mbn wt280" name="srchMarketTypes" id="ca_srchMarketTypeSw"></select>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10 half flleft">
							<p class="fieldName">年代</p>
							<div class="fieldBox">
								<select multiple class="mbn wt280" name="srchAgeTypes" id="ca_srchAgeTypeSw"></select>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<hr>

						<div class="fieldUnit">
							<p class="fieldName">比較方法</p>
							<div class="fieldBox">
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
						<div class="fieldUnit mrgt-10 mrgl30">
							<div class="fieldBox noName">
								<ul>
									<li><label class="radio"><input type="radio" name="ca_srchCompMethod" value="1" data-toggle="radio" checked>前年比</label></li>
								</ul>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
						<div class="fieldUnit mrgl30">
							<div class="fieldBox noName">
								<ul>
									<li><label class="radio"><input type="radio" name="ca_srchCompMethod" value="2" data-toggle="radio">先週比</label></li>
								</ul>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
						<div class="fieldUnit mrgl30">
							<div class="fieldBox noName">
								<ul>
									<li><label class="radio"><input type="radio" name="ca_srchCompMethod" value="3" data-toggle="radio">比較構成比</label></li>
								</ul>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>
						<div class="indent flleft"></div>
						<div class="fieldUnit mrgb10 flleft" id="ca_srchCompOrgID_div">
							<p class="fieldName">比較対象</p>
							<div class="fieldBox">
								<input type="text" class="form-control wt280 flleft cl_valid cl_store" id="ca_srchCompOrgID"/>
								<button class="btn btn-default wt140 flleft mrgl10" id="ca_btn_compare_select" tabindex="-1">参照...</button>
								<label class="checkbox ib mrgl10" for="check1"><input type="checkbox" value="" name="srchCompExistSw" id="ca_srchCompExistSw" data-toggle="checkbox">既存店のみ表示</label>
							</div>
							<div class="clear"></div>
						</div>
						<div class="fieldUnit mrgb10 flleft">
							<div class="fieldBox noName">
								<!-- ツールチップでヘルプを表示 -->
								<p class="txtInFieldUnit flleft help" id="ca_tp_compare" title="比較対象を入力すると、「構成比」の比較が可能です。"><span>?</span></p>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<hr>

						<div class="fieldUnit required">
							<p class="fieldName">対象期間</p>
							<div class="fieldBox">
								<div class="datepicker_wrap ca_srchFromDate_div"><input class="cl_valid cl_date cl_required ime-disabled" type="text" id="ca_srchFromDate" /></div>
								<p class="txtInFieldUnit pdg flleft">〜</p>
								<div class="datepicker_wrap ca_srchToDate_div"><input class="cl_valid cl_date cl_required ime-disabled" type="text" id="ca_srchToDate" /></div>
							</div>
							<div class="clear"></div>
						</div>
						<div class="clear"></div>

						<div class="fieldUnit">
							<div class="fieldBox mrgt20 noName btnCenter">
								<button class="btn btn-info wt280" id="ca_srch">検索</button>
							</div>
							<div class="clear"></div>
						</div>
					</div>
					<!-- /#ca_srchArea -->
					<button id="searchAgain" class="dispn btn">検索条件を再指定</button>
				</div>

				<div id="result" class="dispn">

				<!-- ページネーション＃１：div.pagination-wrapper だけ設置してください -->
				<!--  <div class="pagination-wrapper"></div>-->

				<div class="mrgb10"></div>
				<div class="unit flright mrgb10">比（％）</div>
				<div class="clear"></div>

				<div id="datagrid">
					<div class="cl_datagrid_container" id="ca_datagrid" style=""></div>
				</div>

				<!-- ページネーション＃１：div.pagination-wrapper だけ設置してください -->
				<!-- <div class="pagination-wrapper"></div> -->

				</div>
				<!-- /#result -->

			</div>
			<!-- /#content -->

			<!-- 下部ボタンパネル：中身は MDBaseView が構築します。空の div#mainVolumnFooter だけ設置してください。 -->
			<div id="mainColumnFooter" class="noLeftColumn">
			</div>
			<!-- /#mainColumnFooter -->

		</div>
		<!-- /#mainColumninBox -->

	</div>
	<!-- /#mainColumn -->

</div>
<!-- /#container -->


</div><!-- /#ca_main -->

<div class="clear"></div>

</body>
