<body class="cl_body_hidden">
<div id="ca_main">

<script src="AMBRV0010.js"></script>

<div id="container">
	<!-- 店舗選択補助画面 -->
	<div id="ca_AMPAV0010_dialog" class="cl_dialog"></div>
	<div id="ca_AMPAV0020_dialog" class="cl_dialog"></div>

	<div id="mainColumn">
	<div id="mainColumninBox" class="noLeftColumn">

		<div id="content">

			<div class="fieldgroup">
				<div id="ca_searchArea" class="fieldgroupInBox search">
					<div class="fieldUnit mrgb10  flleft mrgr20 required">
						<p class="fieldName">対象期間</p>
						<div class="fieldBox">
							<ul>
								<li><label class="radio"><input type="radio" name="ca_srchTermSw" value="1" data-toggle="radio" checked>週報</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchTermSw" value="2" data-toggle="radio">月報</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchTermSw" value="3" data-toggle="radio">半期報</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchTermSw" value="4" data-toggle="radio">年報</label></li>
							</ul>
						</div>
						<div class="clear"></div>
					</div>
					<!-- radioの選択によってカレンダー入力の単位が変化 -->
					<div class="fieldUnit mrgb10 half flleft" id="p_ymd">
						<div class="fieldBox noName">
							<div class="datepicker_wrap"><input class="cl_valid cl_date cl_required ime-disabled" type="text" id="ymd" /></div>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 half flleft dispn" id="p_month">
						<div class="fieldBox noName">
							<select name="info" class="mbn wt280 cl_valid" id="month"></select>
							<!-- <input type="text" class="form-control wt280 cl_valid" id="month"/> -->
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 half flleft dispn" id="p_half">
						<div class="fieldBox noName">
							<select name="info" class="mbn wt280 cl_valid" id="half"></select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgb10 half flleft dispn" id="p_year">
						<div class="fieldBox noName">
							<select name="info" class="mbn wt280 cl_valid" id="year"></select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 flleft half mrgr20 required" id="ca_srchUnitID_div">
						<p class="fieldName">事業ユニット</p>
						<div class="fieldBox">
							<select name="info" class="mbn wt280 cl_valid cl_required" id="ca_srchUnitID"></select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 required">
						<p class="fieldName">店舗</p>
						<div class="fieldBox">
							<input class="form-control wt280 flleft cl_valid cl_store cl_required" type="text" id="ca_srchOrgID"/>
							<button class="btn btn-default wt140 flleft mrgl10" id="ca_btn_org_select" tabindex="-1">参照...</button>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit mrgb10 half">
						<p class="fieldName">担当者</p>
						<div class="fieldBox">
							<input type="text" class="form-control wt280 cl_valid" data-validator="maxlen:6,6 digit" id="ca_srchStaffID"/>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					<div class="fieldUnit flleft mrgb10" id="div_outputLevel">
						<p class="fieldName">出力階層</p>
						<div class="fieldBox flleft">
							<div class="flleft ">
								<label class="checkbox flleft mrgr30" for=""><input type="checkbox" id="ca_fOutStaff" data-toggle="checkbox" checked/>担当者</label>
							</div>
							<div class="flleft ">
								<label class="checkbox flleft mrgr30" for=""><input type="checkbox" id="ca_fOutStore" data-toggle="checkbox" checked />店舗</label>
							</div>
							<div class="flleft ">
								<label class="checkbox flleft mrgr30" for=""><input type="checkbox" id="ca_fOutArea" data-toggle="checkbox" checked />ゾーン</label>
							</div>
							<div class="flleft ">
								<label class="checkbox flleft mrgr30" for=""><input type="checkbox" id="ca_fOutZone" data-toggle="checkbox" checked />地区</label>
							</div>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>

					<div class="fieldUnit">
						<div class="fieldBox mrgt20 noName btnCenter" data-cl_groupid="AMBRV0010">
							<button class="btn btn-info wt280 mrgr10" id="cl_pdf">帳票出力</button>
							<button class="btn btn-info wt280 mrgr10" id="cl_csv">Excelデータ出力</button>
						</div>
						<div class="clear"></div>
					</div>
				</div>

			</div>

		</div>
		<!-- /#content -->

		<!-- <div id="mainColumnFooter" class="noLeftColumn"></div> -->
		<!-- /#mainColumnFooter -->

	</div>
	<!-- /#mainColumninBox -->



	</div>
	<!-- /#mainColumn -->



</div><!-- / #container -->

<div class="clear"></div>

</div>
</body>
</html>
