<body class="cl_body_hidden">
<div id="ca_main">


<div id="container">

	<div id="mainColumn">
	<div id="mainColumninBox" class="noLeftColumn">


		<div id="content">

			<div class="fieldgroup">

				<div id="ca_searchArea" class="fieldgroupInBox search">

					<!-- 事業ユニット -->
					<div class="fieldUnit mrgb10 half flleft required">
						<p class="fieldName">事業ユニット</p>
						<div class="fieldBox" id="ca_srchUnitIDArea">
							<select class="mbn wt280 cl_valid cl_required" name="info" id="ca_srchUnitID"></select>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>


					<div class="fieldUnit mrgb10 mrgr20">
						<div class="fieldBox">
							<ul>
								<li><label class="radio"><input type="radio" name="ca_srchOutputType" value="1" data-toggle="radio" checked>品種別年令別在庫一覧</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchOutputType" value="2" data-toggle="radio">店別品種別年令別在庫一覧</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchOutputType" value="3" data-toggle="radio">品番別年令別在庫一覧</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchOutputType" value="4" data-toggle="radio">店別品番別年令別在庫一覧</label></li>
							</ul>
						</div>
						<div class="clear"></div>
					</div>


					<div class="fieldUnit mrgb10 mrgr20">
						<div class="fieldBox">
							<ul>
								<li><label class="radio"><input type="radio" name="ca_srchReportType" value="1" data-toggle="radio" checked>月報</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchReportType" value="2" data-toggle="radio">週報</label></li>
							</ul>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>


					<div class="fieldUnit mrgr20 mrgb10 flleft required">
						<p class="fieldName bl2">出力対象<br>基準日</p>
						<div class="fieldBox">
							<div class="datepicker_wrap"><input class="cl_date cl_valid cl_required" type="text" id="ca_srchYMD"  data-validator="maxlen:13"/></div>
						</div>
						<div class="clear"></div>
					</div>
					<div class="fieldUnit mrgr20 flleft" style="width:140px">
						<p class="fieldName wt120">対象月／対象週</p>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					
					
					<div class="fieldUnit mrgb10 half flleft">
						<p class="fieldName">品種</p>
						<div class="fieldBox">
							<input type="text" class="cl_valid form-control wt280 cl_clearArea" id="ca_srchItgrp"/>
						</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
					
					<div class="fieldUnit mrgb10 mrgr20 dispn" id="ca_age">
						<p class="fieldName">出力対象年令</p>
						<div class="fieldBox">
							<ul class="ul_v">
								<li><label class="checkbox" for="ca_srchAge"><input type="checkbox" value="" id="ca_srchAge5" data-toggle="checkbox">4年以上</label>
								<li><label class="checkbox" for="ca_srchAge"><input type="checkbox" value="" id="ca_srchAge4" data-toggle="checkbox">3年以上～4年未満</label></li>
								<li><label class="checkbox" for="ca_srchAge"><input type="checkbox" value="" id="ca_srchAge3" data-toggle="checkbox">2年以上～3年未満</label></li>
								<li><label class="checkbox" for="ca_srchAge"><input type="checkbox" value="" id="ca_srchAge2" data-toggle="checkbox">1年以上～2年未満</label></li>
								<li><label class="checkbox" for="ca_srchAge"><input type="checkbox" value="" id="ca_srchAge1" data-toggle="checkbox">1年未満</label></li>

							</ul>
							<div class="clear"></div>
						</div>
					</div>
					<div class="clear"></div>

					<div class="fieldUnit mrgb10 mrgr20" id="ca_season">
						<div class="fieldBox">
							<ul>
								<li><label class="radio"><input type="radio" name="ca_srchSeasonType" value="1" data-toggle="radio" checked>合計</label></li>
								<li><label class="radio"><input type="radio" name="ca_srchSeasonType" value="2" data-toggle="radio">シーズン別</label></li>
							</ul>
							</div>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>


					<!-- 表示ボタン -->
					<div class="fieldUnit">
						<div class="fieldBox mrgt20 noName btnCenter " data-cl_groupid="AMWDV0030">
							<button class="btn btn-info wt280" id="cl_csv">Excelデータ出力</button>
						</div>
						<div class="clear"></div>
					</div>


				</div>
			</div>


		</div>
		<!-- /#content -->

		<!--
