<body class="cl_body_hidden">
	<div id="ca_main">

		<script src="AMBPV0030.js"></script>

		<div id="container">
			<!-- 店舗選択補助画面 -->
			<div id="ca_AMPAV0010_dialog" class="cl_dialog"></div>
			<div id="ca_AMPAV0020_dialog" class="cl_dialog"></div>

			<div id="mainColumn">
				<div id="mainColumninBox" class="noLeftColumn">

					<div id="content">

						<div class="fieldgroup">
							<div id="ca_srchArea" class="fieldgroupInBox search">
								<div class="fieldUnit mrgb10 flleft half mrgr20 required" id="div_ca_unit">
									<p class="fieldName">事業ユニット</p>
									<div class="fieldBox">
										<select class="mbn wt280 cl_valid cl_required" id="ca_srchUnitID" name="srchUnitID"></select>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10 required">
									<p class="fieldName">店舗</p>
									<div class="fieldBox" id="div_ca_store">
										<input type="text" placeholder="" class="form-control wt280 flleft cl_valid cl_required" name="srchStoreID" id="ca_srchStoreID"/>
										<button class="btn btn-default wt140 flleft mrgl10" id="ca_btn_store_select" tabindex="-1">参照...</button>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10 flleft half required">
									<p class="fieldName">対象月</p>
									<div class="fieldBox" id="div_ca_srchMonth">
										<select class="mbn wt280 cl_valid cl_required" id="ca_srchMonth" name="srchMonth"></select>
									</div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit">
									<div class="fieldBox mrgt20 noName btnCenter">
										<button id="ca_srch" class="btn btn-info wt280">検索</button>
									</div>
									<div class="clear"></div>
								</div>
							</div>
							<button id="searchAgain" class="dispn btn">検索条件を再指定</button>
						</div>

						<div id="result" class="dispn">
							<div class="fieldgroup">
								<div class="fieldgroupInBox">

									<div class="fieldUnit mrgb10 mrgr20 flleft " id="div_ca_file">
										<p class="fieldName">添付ファイル</p>
										<div class="fieldBox fleft ca_torikomi_div">
											<span id="ca_label" class="choose form-control wt280 flleft"></span>
											<input type="hidden" id="ca_attachedFileID"/>
										</div>
									</div>
									<div class="fieldUnit mrgb10 flleft" style="padding-top:12px;">
										<span class="wt280 flleft" id="disp_storeID" style="font-size: 13px;"></span>
									</div>
									<div class="clear"></div>

									<p class="mrgt10 alertTip" style="font-size: 13px;">
									※&nbsp;<span>&nbsp;&nbsp;!&nbsp;&nbsp;</span>・・・前年と曜日が異なるため、計画立案に注意が必要</p>

									<div class="flright">
										<span>千円単位(税抜)</span>
									</div>
									<div class="clear"></div>

									<div id="myGrid">
										<div class="cl_datagrid_container" id="ca_datagrid"></div>
									</div>
									<div class="clear"></div>
								</div>
							</div>
						</div>
						<!-- /#result -->

					</div>
					<!-- /#content -->

					<div id="mainColumnFooter" class="noLeftColumn x2"></div>
					<!-- /#mainColumnFooter -->

				</div>
				<!-- /#mainColumninBox -->

			</div>
			<!-- /#mainColumn -->

		</div>
		<!-- / #container -->

		<div class="clear"></div>

	</div>
