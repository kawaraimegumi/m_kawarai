</head>

<body class="cl_body_hidden">
	<div id="ca_main">
		<script src="AMBPV0010.js"></script>

		<div id="container">

			<div id="mainColumn">
				<div id="mainColumninBox" class="noLeftColumn">
					<div id="content">
						<div class="fieldgroup">
							<div id="ca_srchArea" class="fieldgroupInBox search">
								<div class="fieldUnit mrgb10 flleft half mrgr20 required" id="div_ca_unit">
									<p class="fieldName">事業ユニット</p>
									<div class="fieldBox">
										<select class="mbn wt280 cl_valid cl_required" id="ca_srchUnitID" name="srchUnitID"></select>
									</div>
									<div class="clear"></div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10 half flleft mrgr20 required">
									<p class="fieldName">計画</p>
									<div class="fieldBox">
										<select class="mbn wt280 flleft cl_valid cl_required" id="ca_srchPlanType" name="srchPlanType"></select>
									</div>
								</div>
								<div class="fieldUnit mrgb10 half flleft required">
									<p class="fieldName">対象期間</p>
									<div class="fieldBox">
										<select class="mbn wt380 cl_valid cl_required" id="ca_srchPeriod" name="srchPeriod"></select>
									</div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit mrgb10 half flleft mrgr20 required">
									<p class="fieldName">ブランチ</p>
									<div class="fieldBox">
										<p class="txtInFieldUnit flright help" title="調印計画が店舗展開となります。" id="ca_tp_srchBranch"><span>?</span></p>
										<select class="mbn wt280 flleft cl_valid cl_required" id="ca_srchBranch" name="srchBranch"></select>
									</div>

								</div>
								<div class="fieldUnit mrgb10 half flleft">
									<p class="fieldName">バージョン</p>
									<div class="fieldBox" id="div_ca_srchVersion">
										<select class="mbn wt380 flleft" id="ca_srchVersion" name="srchVersion"></select>
									</div>
								</div>
								<div class="clear"></div>

								<div class="fieldUnit">
									<div class="fieldBox mrgt20 noName btnCenter">
										<button id="ca_srch" class="btn btn-info wt280">検索</button>
									</div>
									<div class="clear"></div>
								</div>
							</div>
							<button id="searchAgain" class="dispn btn">検索条件を開く</button>
						</div>

						<div id="result" class="dispn">

							<div class="fieldgroup">
								<div class="fieldgroupInBox">
									<div id="div_ca_csvinput">
									<div class="fieldUnit mrgb10 flleft">
										<div class="fieldBox noName flleft" id="ca_csvinput1">
											<button class="btn btn-default wt280 mrgl10 cl-file-attach" id="ca_csv_uptake">Excelデータアップロード</button>
										</div>
										<div class="clear"></div>
									</div>
									<div class="clear"></div>
									<hr>
									</div>

									<div class="flleft wt280 updateEdit updateBasic" id="div_fUpdThis">
										<label class="checkbox flleft mrgr30" for="">
											<input type="checkbox" id="ca_fUpdThis" data-toggle="checkbox" />当月の日別経準率計画も更新する
										</label>
									</div>
									<div class="clear"></div>


									<div class="flright">
										<span>千円単位(税抜)</span>
									</div>
									<div class="clear"></div>

									<div id="myGrid">
										<div class="cl_datagrid_container" id="ca_datagrid" style="height: 750px;"></div>
									</div>
									<div class="clear"></div>
								</div>
								<!-- /#fieldgroupInBox -->
							</div>
							<!-- /#fieldgroup -->
						</div>
						<!-- /#result -->

					</div>
					<!-- /#content -->

					<!-- 下部ボタンパネル：中身は MDBaseView が構築します。空の div#mainVolumnFooter だけ設置してください。 -->
					<div id="mainColumnFooter" class="noLeftColumn x2"></div>
					<!-- /#mainColumnFooter -->

				</div>
				<!-- /#mainColumninBox -->

			</div>
			<!-- /#mainColumn -->

		</div>
		<!-- / #container -->

		<div class="clear"></div>
	</div>

<script id="HdrCell" type="text/template">
	<div class="cell sizeHdr"<% if(it.spanSize>0){ %> style="width:<%- it.spanSize %>px"<% } %>>
		<div class="grd-tbl">
			<div style="display: table-row;">
<% if(it.templateType==0){ %>
				<div class="grd-cell" style="display:table-cell; vertical-align: middle; padding: 0 12px; ">
					<%= it.label %>
				</div>
<% }else{ %>
				<div class="deleteRow" style="display:table-cell;vertical-align:middle;padding: 0 12px; border-left: 1px solid #E4E6Eb; width:80px;">
					<div style="float: left;"><%- it.value %></div>
					<div class="viewAll">&nbsp;</div>
				</div>
<% } %>
			</div>
		</div>
	</div>
</script>


</body>
</html>
