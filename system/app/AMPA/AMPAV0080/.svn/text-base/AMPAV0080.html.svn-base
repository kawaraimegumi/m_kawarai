<div id="ca_AMPAV0080_main">
<div id="mainColumn">
	<div id="mainColumninBox" class="mainColumninBox">
		<div id="title">
			<h2>帳票作成機能検索</h2>
			<div class="titleInBoxRight">
			</div>
		</div>
		<div id="content">
			<div id="selected" class="AMPAV0080_multi selected">
				<p class="title">選択した内容</p>
				<div class="control">
					<span class="delete" id="AMPAV0080_remove_all_button">すべて削除</span>
					<div class="clear"></div>
				</div>
				<div id="innerScroll">
					<ul id="AMPAV0080_selected_func_table"></ul>
					<script id="AMPAV0080_selected_func_template" type="text/x-jquery-tmpl">
						<li>
							<span>${name}</span>
							<span class="code">${typeName}</span>
							<span class="btn-delete AMPAV0080_remove_button"></span>
						</li>
					</script>
				</div>
			</div>
			<div class="fieldgroup">
				<div id="AMPAV0080_searchArea">
					<div id="search" class="fieldgroupInBox search">
						<div class="fieldUnit">
							<p class="fieldName">機能区分</p>
							<div class="fieldBox">
								<span>
									<select class="wt280 cl_valid" id="AMPAV0080_type"></select>
								</span>
							</div>
						</div>
						<div class="fieldUnit">
							<p class="fieldName">機能名</p>
							<div class="fieldBox">
								<input class="form-control wt280 cl_valid" id="AMPAV0080_name" type="text">
							</div>
						</div>
						<div class="fieldUnit">
							<p class="fieldName">更新者</p>
							<div class="fieldBox">
								<input class="form-control wt280 cl_valid" id="AMPAV0080_upd_user" type="text">
							</div>
						</div>
						<div class="fieldUnit">
							<p class="fieldName">更新日</p>
							<div class="fieldBox">
								<div class="datepicker_wrap">
									<input class="cl_date cl_valid ime-disabled" id="AMPAV0080_upd_date" type="text" data-limit="len:10">
								</div>
							</div>
						</div>
						<div class="fieldUnit">
							<div class="fieldBox mrgt20 noName btnCenter">
								<button class="btn btn-default wt140 mrgr20" id="AMPAV0080_clear_button">クリア</button>
								<button id="AMPAV0080_search_button" class="btn btn-info wt280">検索</button>
							</div>
							<div class="clear"></div>
						</div>
					</div>
				</div>
				<button id="searchAgain" class="dispn btn">検索条件を再指定</button>
			</div>
		<div id="result" class="dispn">
			<div class="fieldgroup">
				<div class="fieldgroupInBox">
					<div class="pagination-wrapper">
						<div class="displaypanel" id="AMPAV0080_pager_1_displaypanel"></div>
						<div class="pagination">
							<ul id="AMPAV0080_pager_1"></ul>
						</div>
					</div>
					<div class="table fieldUnit hta">
					<table class="hilightRow" id="AMPAV0080_func_table">
						<tr>
							<th width="40px">
								<label class="checkbox" for=""><input type="checkbox" data-toggle="checkbox" id="AMPAV0080_chkall"></label>
							</th>
							<th>機能区分</th>
							<th>機能名</th>
							<th>更新者</th>
							<th>更新日</th>
						</tr>
						<tbody id="AMPAV0080_func_tbody"></tbody>
					</table>
					<script id="AMPAV0080_func_template_single" type="text/x-jquery-tmpl">
						<tr>
							<td><label class="radio" for=""><input type="radio" data-toggle="radio" name="AMPAV0080_chk"></label></td>
							<td>${typeName}</td>
							<td>${name}</td>
							<td>${updUserName}</td>
							<td>${updDate}</td>
						</tr>
					</script>
					<script id="AMPAV0080_func_template_multiple" type="text/x-jquery-tmpl">
						<tr>
							<td><label class="checkbox" for=""><input type="checkbox" data-toggle="checkbox" name="AMPAV0080_chk"></label></td>
							<td>${typeName}</td>
							<td>${name}</td>
							<td>${updUserName}</td>
							<td>${updDate}</td>
						</tr>
					</script>
					</div>
					<div class="mrgb20"></div>
					<div class="pagination-wrapper">
						<div class="displaypanel" id="AMPAV0080_pager_2_displaypanel"></div>
						<div class="pagination">
							<ul id="AMPAV0080_pager_2"></ul>
						</div>
					</div>
					<div class="fieldUnit">
						<div class="fieldBox mrgt30 mrgb30 noName btnCenter">
							<button class="btn btn-primary wt280 AMPAV0080_multi" id="AMPAV0080_add_button">チェックしたものを選択内容に追加</button>
						</div>
						<div class="clear"></div>
					</div>
				</div>
			</div>
		</div>
		</div>
		<div id="mainColumnFooter">
			<p class="cancel"><a id="AMPAV0080_cancel_button">キャンセル</a></p>
			<p class="apply mrgCancel"><a id="AMPAV0080_commit_button">確定</a></p>
		</div>
	</div>
</div>
</div>